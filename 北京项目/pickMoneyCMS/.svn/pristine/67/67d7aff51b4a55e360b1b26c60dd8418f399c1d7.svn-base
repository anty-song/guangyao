@keyframes  open{
    from{
        top: 2.5rem;
    }
    to{
        top: 0.1rem;
    }
}

@-webkit-keyframes  open{
    from{
        top: 2.5rem;
    }
    to{
        top: 0.1rem;
    }
}

.adv .ad_img{width: 100%;height: 100%;}

