/**app.wxss**/
.share-img,
.service-img {
  position: fixed;
  right: 0;
  width: 136rpx;
  height: 136rpx;
}
.service-img {
  background-color: transparent;
}
.service-img image {
  width: 136rpx;
  height: 136rpx;
  position: absolute;
  left: 0;
  top: 0;
}
.share-img {
  bottom: 150rpx;
}
.btn-group button::after {
  border: none;
}
.service-img {
  bottom: 25rpx;
}
.action-sheet {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, .6);
}
.action-sheet button {
  font-size: 30rpx;
}
.action-sheet button::after{
  border: none;
}
.sheetCtt {
  position: fixed;
  bottom: 0;
  left: 20rpx;
}
.sheetList {
  border-radius: 10rpx;
  overflow: hidden;
}
.sheetCancel {
  background-color: #888;
  margin-top: 4rpx;
  color: #fff;
}
.signup {
  display: flex;
  align-items: center;
  width: 61rpx;
  height: 101rpx;
  padding: 0 20rpx;
  border-radius: 50%;
  background-color: #fff;
  position: fixed;
  right: 20rpx;
  bottom: 300rpx;
  box-shadow: 0 0 25rpx #888;
  font-size: 30rpx;
  color: #0261b1;
}