.rank-ctt {
  width: 704rpx;
  margin: 0 auto;
}
.rank-ctt > view {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.rank-ctt > view > view {
  flex-grow: 1;
  max-width: 176rpx;
  /* white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis; */
  text-align: center;
}
.rank-ctt .table-head {
  height: 78rpx;
  background-color: #e6e6e6;
  color: #000;
  font-size: 28rpx;
}
.rank-ctt .rank-item {
  min-height: 78rpx;
  background-color: rgba(255, 255, 255, .6);
  color: #000;
  font-size: 26rpx;
}
.rank-ctt .rank-item image {
  height: 43rpx;
  width: 36rpx;
}