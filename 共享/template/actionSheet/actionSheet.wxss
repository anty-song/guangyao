.action-sheet {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, .6);
  z-index: 999;
}
.action-sheet button {
  font-size: 40rpx;
}
.action-sheet button::after{
  border: none;
}
.sheetCtt {
  position: fixed;
  bottom: 0;
  left: 20rpx;
}
.sheetList {
  border-radius: 10rpx;
  overflow: hidden;
}
.sheetCancel {
  margin:16rpx 0;
  color:#4aa1de;
}