AJ,EAAiB","file":"static/js/33.218aad2da1924447d822.js","sourcesContent":["<template>\r\n  <section class=\"rightlist-rwfb\">\r\n    <div class=\"rwtit\">\r\n      <h2>我的任务</h2>\r\n      <a href=\"javascript:;\">发布任务</a>\r\n    </div>\r\n    <div class=\"rwtit2\">\r\n      <div class=\"timesx\">\r\n        <a v-bind:class=\"{active:releaseTaskActive}\" @click=\"goRelease\">我发布的</a>\r\n        <a v-bind:class=\"{active:acceptTaskActive}\" @click=\"goAccept\">我接收的</a>\r\n      </div>\r\n      <select>\r\n        <option value>全部</option>\r\n      </select>\r\n    </div>\r\n\r\n    <!-- 发布任务table -->\r\n    <div class=\"fbtable\" v-if=\"releaseTask\">\r\n      <table>\r\n        <tr>\r\n          <th>标题</th>\r\n          <th>金额(元)</th>\r\n          <th>发布时间</th>\r\n          <th>结束时间</th>\r\n          <th>状态</th>\r\n          <th>操作</th>\r\n        </tr>\r\n        <tr v-for=\"relpay in ReleaseList\" :key=\"relpay.id\">\r\n          <td>\r\n            <h4>{{relpay.task_name}}</h4>\r\n          </td>\r\n          <td>{{relpay.task_amount}}</td>\r\n          <td>{{relpay.add_time}}</td>\r\n          <td>{{relpay.end_time}}</td>\r\n          <td v-if=\"relpay.status == '0'\">未审核</td>\r\n          <td v-else-if=\"relpay.status == '1'\">通过待支付</td>\r\n          <td v-else-if=\"relpay.status == '2'\">已支付</td>\r\n          <td v-else-if=\"relpay.status == '3'\">审核拒绝</td>\r\n          <td v-else>删除</td>\r\n          <td>{{relpay.refused_notice}}</td>\r\n        </tr>\r\n      </table>\r\n    </div>\r\n\r\n    <!-- 接受任务table -->\r\n    <div class=\"jstable\" v-if=\"acceptTask\">\r\n      <table>\r\n        <tr>\r\n          <th>标题</th>\r\n          <th>金额(元)</th>\r\n          <th>发布时间</th>\r\n          <th>结束时间</th>\r\n          <th>发布人</th>\r\n          <th>状态</th>\r\n          <th>操作</th>\r\n        </tr>\r\n        <tr v-for=\"Accept in AcceptList\" :key=\"Accept.id\">\r\n          <td>\r\n            <h4>{{Accept.task_name}}</h4>\r\n          </td>\r\n          <td>{{Accept.task_amount}}</td>\r\n\r\n          <td>{{Accept.add_time}}</td>\r\n          <td>{{Accept.end_time}}</td>\r\n          <td>{{Accept.nickname}}</td>\r\n          <td v-if=\"Accept.status == '0'\">测评中</td>\r\n          <td v-else>已完成</td>\r\n          <td>{{Accept.refused_notice}}</td>\r\n        </tr>\r\n      </table>\r\n    </div>\r\n\r\n    <!-- 没有任务 -->\r\n    <div class=\"notable\" v-if=\"none\">\r\n      <table>\r\n        <tr>\r\n          <th>标题</th>\r\n          <th>金额(元)</th>\r\n          <th>发布时间</th>\r\n          <th>结束时间</th>\r\n          <th>发布人</th>\r\n          <th>状态</th>\r\n          <th>操作</th>\r\n        </tr>\r\n        <tr>\r\n          <td colspan=\"7\">暂无任务</td>\r\n        </tr>\r\n      </table>\r\n    </div>\r\n    <div class=\"releasePage\" v-if=\"isRouterAlive\">\r\n      <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"currentPage\"\r\n        layout=\"prev, pager, next, total\"\r\n        :total=\"total\"\r\n        :pager-count=\"pagercount\"\r\n      ></el-pagination>\r\n    </div>\r\n  </section>\r\n</template>\r\n<script>\r\nexport default {\r\n  name: \"uctask\",\r\n  data() {\r\n    return {\r\n      AcceptList: [], //接受任务列表\r\n      ReleaseList: [], //发布任务列表\r\n      releaseTaskActive: true, //发布样式\r\n      acceptTaskActive: false, //接收样式\r\n      releaseTask: true, //发布任务\r\n      acceptTask: false, //接收任务\r\n      none: false, //无任务\r\n      total: 15,\r\n      currentPage: 1,\r\n      pagercount: 5,\r\n      isRouterAlive: true\r\n    };\r\n  },\r\n  created() {\r\n    this.goReleaselist();\r\n  },\r\n  methods: {\r\n    isreload() {\r\n      this.isRouterAlive = false;\r\n      this.$nextTick(function() {\r\n        this.isRouterAlive = true;\r\n      });\r\n    },\r\n    handleSizeChange(val) {\r\n      this.releaseTask == true && this.acceptTask == false\r\n        ? this.goReleaselist(val)\r\n        : this.goAcceptlist(val);\r\n    },\r\n    handleCurrentChange(val) {\r\n      this.releaseTask == true && this.acceptTask == false\r\n        ? this.goReleaselist(val)\r\n        : this.goAcceptlist(val);\r\n    },\r\n    //切换发布任务\r\n    goRelease() {\r\n      this.releaseTaskActive = true;\r\n      this.acceptTaskActive = false;\r\n      this.releaseTask = true;\r\n      this.acceptTask = false;\r\n      this.currentPage = 1;\r\n      this.goReleaselist();\r\n      this.isreload();\r\n    },\r\n    //切换接受任务\r\n    goAccept() {\r\n      this.releaseTaskActive = false;\r\n      this.acceptTaskActive = true;\r\n      this.releaseTask = false;\r\n      this.acceptTask = true;\r\n      this.currentPage = 1;\r\n      this.goAcceptlist();\r\n       this.isreload();\r\n    },\r\n    //接受任务列表\r\n    goAcceptlist(val) {\r\n      this.$axios\r\n        .post(\"/pcgc/mytask\", {\r\n          action: \"receive\",\r\n          page: val\r\n        })\r\n        .then(res => {\r\n          console.log(this.AcceptList);\r\n          if (res.data.code == 1) {\r\n            this.AcceptList = res.data.data;\r\n            // this.total = res.data.pages;\r\n          }\r\n          if (res.data.code == 2) {\r\n            this.releaseTask = false;\r\n            this.acceptTask = false;\r\n            this.none = true;\r\n          }\r\n        });\r\n    },\r\n    //获取任务列表\r\n    goReleaselist(val) {\r\n      this.$axios\r\n        .post(\"/pcgc/mytask\", {\r\n          action: \"release\",\r\n          page: val\r\n        })\r\n        .then(res => {\r\n          console.log(this.ReleaseList);\r\n          if (res.data.code == 1) {\r\n            this.ReleaseList = res.data.data;\r\n            //  this.total = res.data.pages;\r\n          }\r\n          if (res.data.code == 2) {\r\n            this.releaseTask = false;\r\n            this.acceptTask = false;\r\n            this.none = true;\r\n          }\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.releasePage {\r\n  pos