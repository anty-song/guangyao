*{
    padding: 0;
    margin: 0;
    border: none;
}
html{
    width: 100%;
}
ul{}
ul li{list-style: none;}
body{
    width: 100%;
    overflow: hidden;

}
.content{
    min-height: 100vh;
    position: relative;
    background-image: url("../images/GameLobby/bg.png");
    background-repeat: no-repeat;
    background-size: 100% 100%;
    background-position: center center;
    background-attachment: fixed;
}
.container{
  position: relative;
  width: 100%;
  background: url("../images/GameLobby/dd.png") no-repeat;
  background-size: 100% 50%;
  background-position: 0.6rem 2.8rem;
}
.container>.title{
    text-align: center;
    font-size: 0;
}
.container>.title div{
    padding: 0.3rem 0;
    font-size: 0;
    height: .5rem;
    line-height: .5rem;
}
.container>.title .title_p{
    font-size: .28rem;
    color: #fff;
    font-weight: bold;
    letter-spacing: 2px;
    margin: 0 .1rem;
    display: inline-block;
    vertical-align: top;
}
.container>.title .title_before{
    display: inline-block;
    width: 30%;
    height: 2px;
    background: -webkit-linear-gradient(90deg, rgba(255,255,255,0) 0,rgba(255,255,255,1) 100%); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(90deg, rgba(255,255,255,0) 0,rgba(255,255,255,1) 100%); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(90deg, rgba(255,255,255,0) 0,rgba(255,255,255,1) 100%); /* Firefox 3.6 - 15 */
    background: linear-gradient(90deg, rgba(255,255,255,0) 0,rgba(255,255,255,1) 100%);
}
.container>.title .title_after{
    display: inline-block;
    width: 30%;
    height: 2px;
    background: -webkit-linear-gradient(90deg, rgba(255,255,255,1) 0,rgba(255,255,255,0) 100%); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(90deg, rgba(255,255,255,1) 0,rgba(255,255,255,0) 100%); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(90deg, rgba(255,255,255,1) 0,rgba(255,255,255,0) 100%); /* Firefox 3.6 - 15 */
    background: linear-gradient(90deg, rgba(255,255,255,1) 0,rgba(255,255,255,0) 100%); /* 标准的语法 */
}
.container>.title>img{
    width: 5.5rem;
}
.container .menu>ul{
    position: relative;
}
.container .menu>ul>li{
    width: 3.5rem;
    position: absolute;
}
.container .menu>ul>li>i.flag{
    display: block;
    width: 0.5rem;
    height: 2.03rem;
    position: absolute;
}
.container .menu>ul>li.item1{
    left: auto;
    top: 0.1rem;
    right: 0rem;
    bottom: auto;
    animation-delay: 0s;
    animation-duration: 2s;
}
.container .menu>ul>li.item2{
    left: -0.2rem;
    top: 2.1rem;
    right: auto;
    bottom: auto;
    animation-delay: 0.5s;
    animation-duration: 2s;
}
.container .menu>ul>li.item3{
    left: auto;
    top: 3.4rem;
    right: 0rem;
    bottom: auto;
    animation-delay: 1s;
    animation-duration: 2s;
}
.container .menu>ul>li.item4{
    left: -0.2rem;
    top: 5.6rem;
    right: auto;
    bottom: auto;
    animation-delay: 1.5s;
    animation-duration: 2s;
    z-index: 1;
}
.container .menu>ul>li.item5{
    width: 0.81rem;
    height: 1.65rem;
    top: 6.6rem;
    right: 1.9rem;
    z-index: 2;
    animation-delay: 1.5s;
    animation-duration: 2s;
}
.container .menu>ul>li.item1>i.flag {
    background: url("../images/GameLobby/nd.png") no-repeat;
    background-size: 100% auto;
    left: 0.5rem;
    top: -0.3rem;
}
.container .menu>ul>li.item2>i.flag {
    background: url("../images/GameLobby/fl.png") no-repeat;
    background-size: 100% auto;
    right: 0rem;
    top: -0.9rem;
}
.container .menu>ul>li.item3>i.flag {
    background: url("../images/GameLobby/cj.png") no-repeat;
    background-size: 100% auto;
    left: 0.5rem;
    top: -0.4rem;
}
.container .menu>ul>li.item4>i.flag {
    background: url("../images/GameLobby/xl.png") no-repeat;
    background-size: 100% auto;
    right: 0rem;
    top: -1.3rem;
}
.container .menu>ul>li>a{
    width: 100%;
}
.container .menu>ul>li>img{
    width: 100%;
}

.container .wrapper .boats{

}

.container .wrapper .boat{
    position: absolute;
}

.container .wrapper .boat1{
    width: 0.48rem;
    height: 0.43rem;
    background: url("../images/GameLobby/sc1.png") no-repeat;
    -webkit-background-size: contain;
    background-size: contain;
    left: 3rem;
    top: 3.5rem;
}
.container .wrapper .boat2{
    width: 0.75rem;
    height: 0.75rem;
    background: url("../images/GameLobby/sc2.png") no-repeat;
    -webkit-background-size: contain;
    background-size: contain;
    left: 3rem;
    top: 6.5rem;
}
.container .wrapper .boat3{
    width: 0.87rem;
    height: 0.87rem;
    background: url("../images/GameLobby/sc3.png") no-repeat;
    -webkit-background-size: contain;
    background-size: contain;
    left: 2.2rem;
    top: 10.5rem;
}
.container .land{
    position: absolute;
    right: 3rem;
    top: 9.1rem;
}
.container .land>.redpack{
    width: 0.81rem;
    height: 1.65rem;
    /*animation-fill-mode: both;*/
    /*animation-iteration-count: infinite;*/
    position: absolute;
    top: 0.2rem;
    left: 0.3rem;
    z-index: 2;
}
.redpack img{width: 100%;height: 100%;}
#refuse{position: absolute;bottom: .8rem;left: 28%;width: 1.5rem;height: .5rem;border-radius: .25rem;
    background: #FFF288;border: none;outline: none;color: #F18A74;background-size: 100% 100%;}
    #againBtn{position: absolute;bottom: .8rem;right: 28%;width: 1.5rem;height: 0.5rem;border-radius: .25rem;outline: none;border: none;color: #F92A30;background: url(../images/dialog/continue.png) 0 0 no-repeat;background-size: 100% 100%;}
    .container .land>.redpack>a>img{
        width: 100%;
    }
    .container .land>.land_wrap{
        width: 1.67rem;
        height: 1.18rem;
        position: absolute;
        top: 1.5rem;
        left: 0rem;
    }
    .container .land>.land_wrap>img{
        width: 100%;
    }
