/* pages/voting/voting.wxss */
page{
  background-color: #f3f3f3;
}
.container {
  padding-top: 10rpx;
}
.banner image {
  width: 100%;
  margin-top: -9rpx;
  height: 234rpx;
}
.wrap {
  width: 710rpx;
  margin: 0 auto;
  box-sizing: border-box;
  font-size: 0;
}
.champion {
  background-color: #fff;
  padding: 50rpx 30rpx;
  border-radius: 10rpx;
  font-size: 0;
}
.champion > view {
  display: inline-block;
  vertical-align: top;
  width: 240rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.item .logo-box {
  width: 170rpx;
  display: inline-block;
  border: 1rpx solid transparent;
  margin-left: 80rpx;
  margin-top: 32rpx;
}
.champion .logo-box {
  border: 1rpx solid transparent;
  width: 240rpx;
  box-sizing: border-box;
}
.champion .logo {
  width: 235rpx;
  height: 127.5rpx;
  border: 1rpx solid rgba(2, 97, 177, .5);
}
.champion .rank {
  width: 240rpx;
  text-align: center;
}
.champion .champion-cup {
  width: 92rpx;
  height: 112rpx;
}
.champion .vote-btn {
  float: right;
  margin-top: 56rpx;
  width: 170rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size:24rpx;
  color: #fff;
  background-color: #0261b1;
}
.champion .name {
  font-size: 34rpx;
  line-height: 34rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  margin-top: 30rpx;
  color: #0261b1;
  text-align: center
}
.champion .vote-num {
  font-size: 24rpx;
  line-height: 24rpx;
  color: #4e4e4e;
  text-align: center;
  margin-top: 40rpx;
}
.item {
  position: relative;
  display: inline-block;
  width: 350rpx;
  height: 350rpx;
  background-color: #fff;
  border-radius: 10rpx;
  box-sizing: border-box;
  padding: 10rpx;
  margin-top: 10rpx;
}
.item:nth-child(odd) {
   margin-left: 10rpx; 
}
.second-cup,
.third-cup {
  vertical-align: top;
  width: 44rpx;
  height: 61rpx;
}
.second .logo-box,
.third .logo-box {
  margin-left: 36rpx;
}
.item .logo {
  width: 165rpx;
  height: 90rpx;
  border: 1rpx solid rgba(2, 97, 177, .5);
}
.item .name {
  font-size: 30rpx;
  line-height: 30rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  margin-top: 40rpx;
  text-align: center;
  color: #000;
}
.item .vote-num {
  font-size: 24rpx;
  line-height: 24rpx;
  margin: 30rpx 0;
  text-align: center;
}
.item .vote-num text {
  color: #d50500;
}
.item .vote-btn {
  width: 170rpx;
  height: 50rpx;
  line-height: 50rpx;
  background-color: #0261b1;
  font-size: 20rpx;
  color: #fff;
  margin: 0 auto;
}
.bg-color {
  background-color: #fffbec;
   box-shadow: 0 0 10rpx 0 #8d8c88; 
}
.front-rank-box,
.back-rank-box {
  position: absolute;
  top: 0;
  left: 10rpx;
  height: 41rpx;
  text-align: center;
  line-height: 41rpx;
}
.front-rank-pic,
.back-rank-pic {
  position: absolute;
  top: 0;
  left: 0;
  height: 41rpx;
}
.front-rank-box,
.front-rank-pic {
  width: 39rpx;
}
.back-rank-box,
.back-rank-pic {
  width: 32rpx;
}
.front-rank-box > text,
.back-rank-box > text {
  display: block;
  text-align: center;
  position: absolute;
  top: 0;
  left: 0;
  color: #fff;
}
.front-rank-box > text {
  font-size: 20rpx;
  width: 39rpx;
}
.back-rank-box > text {
  font-size: 16rpx;
  width: 32rpx;
}
.vote-result,
.mark {
  position: fixed;
  width: 100%;
  height: 100%;
  padding-top: 270rpx;
  top: 0;
  left: 0;
  background-color: rgba(0,0,0,0.7);
}
.mark {
  z-index: 999;
}
.vote-result {
  z-index: 9;
}
.vote-result-ctt,
.mark-ctt {
  position: relative;
  display: flex;
  display: -webkit-flex;
  flex-flow: column;
  align-items: center;
  width: 500rpx;
  background-color: #fff;
  border-radius: 10rpx;
  margin: 0 auto;
}
.mark .close {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 20rpx;
  right: 20rpx;
}
.mark .mark-logo {
  width: 240rpx;
  height: 120rpx;
  margin-top: 30rpx;
  border: 1rpx solid rgba(2, 97, 177, .5);
  box-sizing: border-box;
}
.mark .hold {
  font-size: 24rpx;
  color: #353535;
  margin: 30rpx 0 40rpx 0;
}
.mark .verify-box {
  display: flex;
  width: 450rpx;
  margin: 0 auto;
  flex-flow: row;
  justify-content: space-between;
}
.mark .verify-box .show-code {
  width: 160rpx;
  height: 50rpx;
  background-color: #d6e4f0;
  border-radius: 10rpx;
  margin-left: 20rpx;
  overflow: hidden;
}
.mark .verify-box .show-code image {
  width: 100%;
  height: 100%;
}
.mark .verify {
  width: 210rpx;
  height: 50rpx;
  border: 1rpx solid #888;
  font-size: 24rpx;
  border-radius: 10rpx;
  box-sizing: border-box;
  padding-left: 20rpx;
}
.vote-result button,
.mark button {
  font-size: 24rpx;
  line-height: 50rpx;
  width: 260rpx;
  height: 50rpx;
  margin: 50rpx 0;
  color: #fff;
}
.mark button {
  background-color: #0261b1;
}
.vote-result button {
  margin-top: 0;
}
.vote-result .vote-result-pic {
  position: absolute;
  top: -50rpx;
  width: 100rpx;
  height: 100rpx;
}
.vote-result .link {
  display: block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  height: 50rpx;
  width: 480rpx;
  font-size: 22rpx;
  line-height: 50rpx;
  background-color: #fff2db;
  border-radius: 10rpx;
}
.vote-result .name,
.vote-result .link-msg {
  font-size: 26rpx;
  line-height: 26rpx;
  color: #0261b1;
}
.vote-result .tip {
  font-size: 30rpx;
  line-height: 30rpx;
  margin: 75rpx 0 28rpx 0;
}
.vote-result .link {
  margin: 19rpx 0 30rpx 0;
}
.vote-result .name text {
  color: #d50500;
}