*{padding: 0;margin: 0;border: none;}
document,body{width: 100%;height: 100%;}
#game_wrapper{width: 100%;height: 100%;}
#content{overflow: hidden;width: 100%;height: 100%;}
#canvas_bg{position: absolute;left: 0;top: 48px;}
#canvas_elf{position: absolute;left: 0;top: 48px;z-index: 99;}
.rules{
	padding: 5px 15px;
	border-radius: 20px;
	text-align: center;
	position: fixed;
	font-size: 14px;
	color: #FFFFFF;
	top: 60px;
	z-index: 100;
	left: 12px;
	background: rgba(0,0,0,.3);
}
.my_price{
	position: fixed;
	z-index: 100;
	top: 48px;
	right: 0px;
	width: 50px;
	height: 50px;
	background: url("../images/Tongs/myprice.png") no-repeat;
	background-size: contain;
}
#toast{
	width: 300px;
	height: 300px;
	display: block;
	transition: .5s;
	background: #FFFFFF;
	position: fixed;
	top: 50%;
	left: 50%;
	transform: translate(-50%,-50%);
}
.nextgame{position: absolute;display: none;z-index: 99;bottom: 6%;width: 24%;right: 0;-webkit-animation: mymove 3s infinite linear;animation: mymove 3s infinite linear;}
.tips_box{position: absolute;z-index: 99;margin: auto;top: 30%;width: 100%;}
.tips_box p{font-size: .24rem;color: #FFFFFF;text-align: center;background: #191970;width: 36%;margin: auto;border-radius: 0.08rem;}

.notice{width: 3.2rem;height: .74rem;margin:auto;top: 1.22rem; background: transparent;overflow: hidden;position: absolute;z-index: 100;left: 0;right: 0;}
.notice_box{position: relative;height: .74rem;width: 100%;overflow: hidden;}
.notice_list{position: absolute;margin: 0;padding: 0;width: 100%;height:1.26rem;left: 0;top: 0;font-size: 0;}
.notice_item{width: 100%;height:.74rem;display: inline-block;}
.notice p{line-height: .8rem;margin: 0;text-align: center;font-size: .26rem;color: #FFFFFF;white-space: nowrap;overflow: hidden; text-overflow: ellipsis;}
@keyframes mymove {
    0%  {right:-.33rem;}
    1%  {right:-.33rem;transform: rotate(-20deg);-webkit-transform: rotate(-20deg);}
    40% {right: -.33rem;transform: rotate(-20deg);-webkit-transform: rotate(-20deg);}
    41% {right: -.33rem;transform: rotate(0deg);-webkit-transform: rotate(0deg);}
    60% {right: .5rem;transform: rotate(0deg);-webkit-transform: rotate(0deg);}
    65% {right: .5rem;transform: rotate(-20deg);-webkit-transform: rotate(-20deg);}
    70% {right: .5rem;transform: rotate(20deg);-webkit-transform: rotate(20deg);}
    75% {right: .5rem;transform: rotate(-20deg);-webkit-transform: rotate(-20deg);}
    80% {right: .5rem;transform: rotate(0deg);-webkit-transform: rotate(0deg);}
    100% {right: -.33rem;transform: rotate(0deg);-webkit-transform: rotate(0deg);}
}

@-webkit-keyframes mymove{
 0%  {right:-.33rem;}
    1%  {right:-.33rem;transform: rotate(-20deg);-webkit-transform: rotate(-20deg);}
    40% {right: -.33rem;transform: rotate(-20deg);-webkit-transform: rotate(-20deg);}
    41% {right: -.33rem;transform: rotate(0deg);-webkit-transform: rotate(0deg);}
    60% {right: .5rem;transform: rotate(0deg);-webkit-transform: rotate(0deg);}
    65% {right: .5rem;transform: rotate(-20deg);-webkit-transform: rotate(-20deg);}
    70% {right: .5rem;transform: rotate(20deg);-webkit-transform: rotate(20deg);}
    75% {right: .5rem;transform: rotate(-20deg);-webkit-transform: rotate(-20deg);}
    80% {right: .5rem;transform: rotate(0deg);-webkit-transform: rotate(0deg);}
    100% {right: -.33rem;transform: rotate(0deg);-webkit-transform: rotate(0deg);}
}
