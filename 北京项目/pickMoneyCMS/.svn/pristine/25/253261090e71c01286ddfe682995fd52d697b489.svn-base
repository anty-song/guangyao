*{padding: 0;margin: 0;}
body{background: rgba(254,66,71,1);width: 100%;min-height: 100vh;position: fixed;overflow-x: hidden;}
.header{width: 100%;padding-top: 0.15rem;}
.header img{width: 100%;height:3.45rem;}
.content{width: 7.5rem;height: 4rem;background: url("../images/Lottery/game_bg.png") no-repeat center -.35rem/100% 110%;overflow: visible;margin-top: -0.24rem;position: relative;}
.game_wrap{
    width: 6.5rem;
    height: 2.8rem;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    -webkit-transform: translate(-50%,-50%);
    -moz-transform: translate(-50%,-50%);
    -o-transform: translate(-50%,-50%);
    -webkit-background-size: 100% 100%;
    background-size: 100% 100%;
}
#canvas{
    width: 6.5rem;
    height: 2.8rem;
}

.start{
    position: absolute;
    z-index: 10;
    width: 107%;
    height: 130%;
    border-radius: 0.45rem;
    border: none;
    outline: none;
    left: 50%;
    top: 64%;
    transform: translate(-50%,-63%);
    -webkit-transform: translate(-50%,-63%);
    -moz-transform: translate(-50%,-63%);
    -o-transform: translate(-50%,-63%);
    background: url(../images/Lottery/startBtn.png) center center no-repeat;
    -webkit-background-size: 3.2rem 0.9rem;
    background-size: 3.2rem 0.9rem;
    transition: top.5s;
}
.start_tips_box{
    height: .5rem;
    width: 100%;
    background: #F12B31;
    margin-top: -0.3rem;
    position: absolute;
}
.start_tips{
    font-size: .24rem;
    text-align: center;
    color: #FFF;
}
.start_tips strong{
   margin: 0 .04rem;
}
.redpack_logo{position: absolute;
    width: 22%;
    right: 0;
    top: 8.2rem;
    z-index: 99;
    display: none;
    -webkit-animation: mymove 3s infinite linear;
    animation: mymove 3s infinite linear;
}
.redpack_logo img{width: 100%;}
.menus{position: relative;}
.menus .head_img{
    position: absolute;
    z-index: 2;
    width: 0.8rem;
    font-size: 0;
}

.menus .head_img img{
    width: 100%;
}
.menus .slide_image{
    width: 100%;
    height: 1.8rem;
    line-height: 1.8rem;
    margin: auto;
    text-align: center;
    background:#fff;
}

.menus .slide_image img{
    max-width: 75%;
    max-height: 1.4rem;
    vertical-align: middle;
}

.menus .swiper-slide .slide_text{
    width:110%;
    position: absolute;
    text-align: center;
    left: -5%;
    right: -5%;
    height: 0.4rem;
    line-height: 0.4rem;
    background: #fe4247;
}

.menus .swiper-slide .slide_text p{
    color: #FFF;
    text-align: center;
    font-size: .24rem;
}

.rules{
    position: absolute;
    top: 1.1rem;
    left: .2rem;
    padding: .12rem .36rem;
    background: rgba(255,255,255,.5);
    color: #FFFFFF;
    font-size: .24rem;
    border-radius:.55rem;
    z-index: 5;
}
.my-price{
    width: 1.4rem;
    position: absolute;
    right: 0rem;
    top: .88rem;
    z-index: 5;
}
.my-price img{
    width: 100%;
}
.price_wrapper {
    height: 100%;
    overflow-x: scroll;
    padding: .3rem .3rem 0 .3rem;
    margin-top: .2rem;
}
.price_list{
    width: 16.7rem;
    font-size: 0;
}
.price_item{
    display: inline-block;
    width: 1.88rem;
    height: 2.2rem;
    border: 0.05rem solid #c22428;
    -webkit-border-radius: 0.1rem;
    -moz-border-radius: 0.1rem;
    border-radius: 0.1rem;
    box-sizing: border-box;
    overflow: hidden;
    margin-right: .2rem;
}
.price_item .slide_image{
    width: 100%;
    height: 1.7rem;
    background: #fff;
}
.price_item .slide_text{
    height: .4rem;
    line-height: .5rem;
    font-size: .28rem;
    color: #fff;
    text-align: center;
}
.notice{width: 4.98rem;height: .7rem;margin:-.24rem auto 0;padding: 0 0 .24rem;background: url(../images/Lottery/notice_bg.png) no-repeat center/100% 100%;overflow: hidden;}
.notice_box{position: relative;height: .7rem;width: 100%;overflow: hidden;}
.notice_list{position: absolute;margin: 0;padding: 0;width: 100%;height:1.2rem;left: 0;top: 0;font-size: 0;}
.notice_item{width: 100%;height:.74rem;display: inline-block;}
.notice p{line-height: .75rem;margin: 0;text-align: center;font-size: .26rem;color: #f56447;white-space: nowrap;}

@keyframes mymove {
    0%  {right:-.33rem;}
    1%  {right:-.33rem;transform: rotate(-20deg);-webkit-transform: rotate(-20deg);}
    40% {right: -.33rem;transform: rotate(-20deg);-webkit-transform: rotate(-20deg);}
    41% {right: -.33rem;transform: rotate(0deg);-webkit-transform: rotate(0deg);}
    60% {right: .5rem;transform: rotate(0deg);-webkit-transform: rotate(0deg);}
    65% {right: .5rem;transform: rotate(-20deg);-webkit-transform: rotate(-20deg);}
    70% {right: .5rem;transform: rotate(20deg);-webkit-transform: rotate(20deg);}
    75% {right: .5rem;transform: rotate(-20deg);-webkit-transform: rotate(-20deg);}
    80% {right: .5rem;transform: rotate(0deg);-webkit-transform: rotate(0deg);}
    100% {right: -.33rem;transform: rotate(0deg);-webkit-transform: rotate(0deg);}
}

@-webkit-keyframes mymove{
 0%  {right:-.33rem;}
    1%  {right:-.33rem;transform: rotate(-20deg);-webkit-transform: rotate(-20deg);}
    40% {right: -.33rem;transform: rotate(-20deg);-webkit-transform: rotate(-20deg);}
    41% {right: -.33rem;transform: rotate(0deg);-webkit-transform: rotate(0deg);}
    60% {right: .5rem;transform: rotate(0deg);-webkit-transform: rotate(0deg);}
    65% {right: .5rem;transform: rotate(-20deg);-webkit-transform: rotate(-20deg);}
    70% {right: .5rem;transform: rotate(20deg);-webkit-transform: rotate(20deg);}
    75% {right: .5rem;transform: rotate(-20deg);-webkit-transform: rotate(-20deg);}
    80% {right: .5rem;transform: rotate(0deg);-webkit-transform: rotate(0deg);}
    100% {right: -.33rem;transform: rotate(0deg);-webkit-transform: rotate(0deg);}
}
