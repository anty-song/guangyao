    margin-bottom: .4rem;
}
.mylayer p,
.mylayer1 p,
.mylayer2 p{
    margin-bottom: .2rem;
    color: #333;
    font-size: .18rem;
    font-weight: 400;
}
.copy{
    font-size: .2rem;
    display: inline-block;
    width: 4.6rem;
    height: .5rem;
    line-height: .5rem;
    color: #fff;
    text-align: center;
    margin-top: .5rem;
    background-color: #0084ff;
    outline: none;
    border: none;
    cursor: pointer;
}
.ptitle{
    margin-right: .2rem;
}
.pvalue2{
    color: #0084ff;
    display: inline-block;
    width: 3rem;
}
.shadow{
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;left: 0;bottom: 0;right: 0;
    background: rgba(0,0,0,0.2);
    z-index: 10;
    display: none;
}

