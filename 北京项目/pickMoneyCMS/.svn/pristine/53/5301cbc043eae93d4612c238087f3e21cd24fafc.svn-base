.layui-header{height: 90px;}
.logo_wrapper{height: 90px;line-height: 90px;}
.logo_wrapper i{font-size: 20px;}
.logo{margin-top: -20px;margin-right:20px;}
.logo_title{color: #fff;font-size: 24px;margin-right: 30px;}
.layui-header .layui-nav-item{text-align: center;padding-top: 15px;line-height: 30px;padding-bottom:7px;}
.layui-header p i{font-size: 36px;color: rgba(255,255,255,.7);}
.help_tips a, .help_tips span{font-size: 18px;}
.help_tips .shu{border-right: 2px dotted #eee;margin: 0 10px;}
.help_tips a{color: #fff;}
#demo{text-align: center;}
.showitem{display: block;}
.rotateitem{transform-origin: 50% 25% 0;transform: rotate(180deg);}
.copyright{text-align: center;}
.bac{width: 100%;height: 100%;background: #000;opacity: .5;position: fixed;top:0;left:0;display: none;}
iframe{width: 100%;height: 100%;position: absolute;}
/*去掉type=number的input框的上下箭头*/
input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button {-webkit-appearance: none;  }
input[type="number"]{-moz-appearance: textfield;  }
@media(max-width:992px){
    .help_tips a{margin-right: 20px;}
    .layui-side{transform: translate(-220px,0);-webkit-transform: translate(-220px,0);width: 220px;}
    .layui-body{left:0px;}
    .layui-footer{left:0px!important;}
    .person_wrapper li{line-height:73px!important;height:73px;}
    .layui-form-label:not(:first-child){margin-top: 5px;}
    .layui-form label{width: 100px;}
    .layui-laypage a, .layui-laypage span{
        font-size: 14px!important;height: 20px;line-height:20px;padding:0 5px;
    }
    iframe{width: 100%;left: 0;}
    .btnWrapper{margin-left: 0!important;}
    .layui-btn-container{margin-left: 0!important;}
    textarea{margin-left: 0!important;}
    .layui-table td,.layui-table th{padding: 2px 15px}
    .fanwei{overflow: hidden;}
    .fanwei div{float: left!important;margin-right: 5px!important;margin-left: 1px!important;}
    .layui-form-mid{top:0!important;margin-bottom: 10px;}
    .bwhb .layui-input-inline{left:0!important;}
    .layui-btn+.layui-btn{margin-left: 0;}
    .help_tips dd a{font-size: 14px;}
    .layui-nav-child{line-height: 28px;}
}
textarea{width: 100%;padding: 10px;box-sizing: border-box;}
.layui-form-label{overflow:hidden;white-space: nowrap;text-overflow: ellipsis;min-width: 115px;}
label span{color: red;}
.layui-input-block{margin-left: 36px;}
.layui-table{text-align: center;}
.layui-table th{text-align: center;}
.gggl .layui-table th{text-align: center;background: #fff;}
.per{float: right;margin-left: 15px;}
.addyh{float: right;height: 100%;}
.layui-colla-content{overflow-x:auto;}
.layui-icon{margin-right: 10px;}
.deladver:hover{color: red!important;cursor: pointer;}
.deletetr:hover{color: red!important;cursor: pointer;}
.bwhb .layui-colla-item p i{color: blue;}
.whoname{color: red;}
.whatname{color: blue;}
#test-upload-more-list img{max-width: 100px;max-height: 100px;}
.mark{position: fixed;top: 0;left: 0;right: 0;bottom: 0;background: #000;z-index: 999;opacity: .6;display: none;}
.create-question{position: fixed;top:25%;left:35%;width: 500px;height: 370px;display: none;z-index: 1000;background: #fff;text-align: center;border-radius: 5px;}
.create-question p{text-align: center;margin: 10px auto;font-size: 20px;}
.create-question .closeModel{position: absolute;right: -40px;top: -50px;width: 30px;height: 30px;cursor: pointer;cursor: hand;}
.createUpload{width:120px;height:40px;}

/*商家logo管理*/
.sjlogo .logo-model{position: absolute;top: 30%;left: 38%;width: 400px;background: #fff;height: 350px;border-radius: 10px;overflow: hidden;display: none;z-index: 1000;}
.sjlogo .layui-colla-title h2{float: left;font-size: 16px;}
.sjlogo .layui-colla-title form{float: right;}
.sjlogo .model-title{display: flex;justify-content: space-between;height: 40px;background: #ddd;line-height: 40px;padding: 0 20px;}
.sjlogo .model-body{padding: 20px;}
.sjlogo .model-body .img-wrapper{position: relative;width:100px;height:100px;line-height: 100px;text-align: center;border:1px solid #ddd;border-radius: 5px;font-size:0;}
.sjlogo .model-body .img-wrapper input{position: absolute;width: 100%;height: 100%;top: 0;left: 0;z-index: 1001;opacity: 0;}
.sjlogo  #show{max-width: 100%;max-height:100%;}
.sjlogo .adver-model{position: absolute;top:0;left: 0;bottom: 0;right: 0;background: #fff;padding:60px 10%;width:100%;min-height: 200%;display: none;box-sizing: border-box;}
.sjlogo .adver-back{position: absolute;left: 60px;top: 20px;font-size: 17px;color: #0af;cursor: pointer;}
.sjlogo #createAdver label{min-width:120px;text-align: right;}
.adDetail #adverDtail label{min-width:120px;text-align: right;}

.totaltr{width: 20px;height: 20px;vertical-align:middle;}
#batch_operation{width: 90px;height: 24px;vertical-align: middle;border-radius: 5px;font-size: 12px;color: #777;margin: 0 30px 0 5px;}
.layui-form-switch em{display: inline-block;}
.sjgggl .layui-colla-title .s1{color: #00d200;}
.sjgggl .layui-colla-title .s2{color: #bdb00b;}
.sjgggl .layui-colla-title .s3{color: red;}
.sjgggl .layui-colla-title .s8{color: blue;}

/*数据统计部分*/
.staticList .layui-card-header{font-size: 18px;font-weight: bold;}
.staticList .layui-tab>a{position: absolute;right: 20px;top: 10px;font-weight: bold;}
.staticList .layui-tab>a i{font-size: 20px;}
.staticList .layui-card-body .staticItem .layuiadmin-cart-text{background-color:rgb(248,248,248);padding: 10px 20px;}
.staticList .staticItem .layuiadmin-cart-text .layui-text-top{color: #999;font-size: 14px;margin-bottom: 10px;}
.staticList .staticItem .layuiadmin-cart-text .layui-text-center{font-style: normal;font-size: 30px;font-weight: 300;color: #009688;}

/*趣味游戏-福利彩票*/
.flcp .layui-table input{width: 20px;height: 20px;vertical-align: middle;}
.flcp .layui-collapse h2{font-size: 14px;display: inline-block;}

/*用户管理-信息库配置*/
.xxkpz table input{width: 20px;height: 20px;vertical-align: middle;}

/*百万红包*/
.bwhb .ee{background: #eee;}
.bwhb .ff{background: #fff;border: 1px solid #f78559!important;}
.bwhb .fanwei>div{margin-bottom: 10px;}