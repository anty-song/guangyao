.open-group {
  width: 684rpx;
  margin: 0 auto;
  display: flex;
  flex-wrap: wrap;
}
.open-group .group-item {
  width: 208rpx;
  height: 46rpx;
  border-radius: 4rpx;
  border: 1rpx solid #dcdcdc;
  color: #8a8a8a;
  background-color: #fff;
  font-size: 24rpx;
  margin-bottom: 24rpx;
  text-align: center;
  line-height: 46rpx;
}
.open-group .group-item:nth-child(3n + 2) {
  margin: 0 24rpx;
}
.search {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 18rpx;
}
.search .search-input {
  width: 539rpx;
  height: 61rpx;
  border: 2rpx solid #bebebe;
  border-radius: 10rpx;
  padding-left: 14rpx;
  color: #a9a9a9;
  font-size: 24rpx;
  /* box-shadow: -5rpx 5rpx 5rpx 0 #ccc inset; */
  background-color: #fff;
}
.search .search-btn {
  height: 60rpx;
  width: 128rpx;
  color: #fff;
  border-radius: 10rpx;
  text-align: center;
  font-size: 24rpx;
  line-height: 60rpx;
}
.player {
  width: 670rpx;
  margin: 16rpx auto 0;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.player .item {
  position: relative;
  width: 320rpx;
  border-radius: 10rpx;
  /* overflow: hidden; */
  margin-bottom: 35rpx;
}
.player .item .number {
  position: absolute;
  top: -5rpx;
  left: -5rpx;
  width: 90rpx;
  height: 40rpx;
  background-color: rgba(0, 0, 0, .4);
  border-top-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
  color: #fff;
  font-size: 24rpx;
  text-align: center;
  line-height: 40rpx;
}
.player .item .img-box image {
  height: 100%;
  width: 100%;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
}
.player .item .item-info {
  height: 172rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  color: #fff;
  font-size: 26rpx;
}
.player .item .item-info .name {
  max-width: 248rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.player .item .item-info .name .icon {
  margin-right: 3rpx;
}
.player .item .item-info .handle {
  height: 70rpx;
  width: 248rpx;
  background-color: #fff;
  color: #333;
  text-align: center;
  line-height: 70rpx;
  border-radius: 12rpx;
}
.player .item .item-info .handle .icon {
  margin-right: 7rpx;
}
.yzm,
.ling,
.vote-result {
  display: flex;
  align-items: center;
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  background-color: rgba(0,0,0,0.7);
  z-index: 999;
}
.yzm-ctt {
  position: relative;
  display: flex;
  display: -webkit-flex;
  flex-flow: column;
  align-items: center;
  width: 520rpx;
  padding: 48rpx 45rpx 28rpx;
  background-color: #fff;
  border-radius: 10rpx;
  margin: 0 auto;
}
.yzm .yzm-tit {
  font-size: 30rpx;
  line-height: 30rpx;
  color: #4d4d4d;
  margin-bottom: 56rpx;
}
.yzm .yzm-input {
  width: 520rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 20rpx;
}
.yzm .yzm-input input {
  width: 250rpx;
  font-size: 26rpx;
  border-bottom: 1rpx solid #999;
}
.yzm .yzm-img {
  width: 254rpx;
  height: 92rpx;
}
.ling .err-msg,
.yzm .err-msg {
  font-size: 26rpx;
  line-height: 26rpx;
  color: #ff6363;
  align-self: flex-start;
}
.ling .ling-btn,
.yzm .yzm-btn {
  width: 200rpx;
  height: 80rpx;
  border-radius: 10rpx;
  background-color: #619fd9;
  color: #fff;
  font-size: 26rpx;
  line-height: 80rpx;
  border: 1rpx solid #2c5f8f;
  margin: 38rpx auto 0;
}
.ling-ctt {
  position: relative;
  display: flex;
  display: -webkit-flex;
  flex-flow: column;
  align-items: center;
  width: 500rpx;
  padding: 90rpx 55rpx 28rpx;
  background-color: #fff;
  border-radius: 10rpx;
  margin: 0 auto;
}
.ling .ling-tit {
  font-size: 28rpx;
  line-height: 28rpx;
  color: #4d4d4d;
}
.ling input {
  width: 500rpx;
  border-bottom: 1rpx solid #999;
  margin: 30rpx 0;
  text-align: center;
}
.yzm image,
.ling image,
.vote-result image {
  width: 100%;
  height: 100%;
}
.yzm .close image,
.ling .close image,
.vote-result .close image {
  width: 30rpx;
  height: 30rpx;
}
.vote-result .vote-result-ctt {
  position: relative;
  display: flex;
  display: -webkit-flex;
  flex-flow: column;
  align-items: center;
  width: 440rpx;
  padding: 44rpx 50rpx 36rpx;
  background-color: #fff;
  border-radius: 10rpx;
  margin: 0 auto;
}
.yzm .close,
.ling .close,
.vote-result .close {
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  top: 0;
  right: 0;
  padding: 30rpx 30rpx 0 0;
  text-align: right;
  vertical-align: top;
}
.vote-result .img-box {
  width: 135rpx;
  height: 101rpx;
}
.vote-result .reshint {
  font-size: 30rpx;
  line-height: 30rpx;
  margin: 27rpx 0 30rpx;
}
.vote-result .thank {
  font-size: 26rpx;
  line-height: 28rpx;
  text-align: center;
}
.vote-result .msg {
  text-align: center;
  font-size: 20rpx;
  line-height: 20rpx;
  margin: 20rpx 0 30rpx;
  color: #888;
}
.vote-result .lapiao {
  font-size: 24rpx;
  line-height: 24rpx;
  margin-bottom: 24rpx;
  color: #1e478c;
}
.vote-result .btn-group {
  width: 440rpx;
  display: flex;
  justify-content: space-between;
}
.vote-result .btn-group button {
  width: 200rpx;
  height: 80rpx;
  border-radius: 10rpx;
  color: #fff;
  font-size: 28rpx;
  line-height: 80rpx;
  text-align: center;
  background-color: #619fd9;
  border: 1rpx solid #2c5f8f;
}
.empty {
  width: 229rpx;
  height: 278rpx;
  margin: 100rpx auto;
}
.empty image {
  width: 100%;
  height: 100%;
}
.active-intro {
  font-size: 25rpx;
  line-height: 40rpx;
  padding: 30rpx 20rpx 0; 
  width: 566rpx;
  margin: 0 auto;
  border-radius: 10rpx;
  background-color: rgba(255,255,255,.7);
  box-shadow: 0 0 10rpx 0 #fff;
}
.rules-detail-tit {
  display: inline-block;
  padding-bottom: 5rpx;
  margin-bottom: 11rpx;
}
.active-intro {
  padding-bottom: 30rpx;
}
