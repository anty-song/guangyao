//点击浇花
$(".draw_item").click(function () {
	var that = $(this);
	price_time=parseInt($(".tips_box p strong").text());
	if (price_time == 0) {
		$(".ui-mask").fadeIn();
		$(".dialog_end").show();
		clearInterval(ticks);
		ticks = setInterval(function () {
			if (cha > 0) {
				$('.dialog_end .curtime').text(leftTime(cha));
			} else {
				clearInterval(ticks);
				window.location.reload();
			}
			cha--;
		}, 1000);
	}else{
    	//	前后端交互返回中奖情况
    	key_auth=auth.substring(0,8);
    	time=Math.round(new Date() / 1000);
    	paramData={'uid':uid,'time':time,'auth':auth,'device':device,'game_type':g_id};
    	paramData=JSON.stringify(paramData);
    	paramData=encryptByDES(paramData,key_auth);
    	var random;
    	$.ajax({
    		type:water.water.type,
    		url:water.url,
    		data:{action:water.water.action,uid:uid,reqType:2,paramData:paramData},
    		dataType:'json',
    		success:function(data){
    			var data=decryptByDES(data.res_data, key_auth);
    			data=JSON.parse(data);
    			console.log(data);
    			cha=data.count_down;
    			p_id=data.p_id;
    			ad_url=data.entry_game_popout.ad_url;
    			if(data.num==0){
    				ticks = setInterval(function () {
    					if (cha > 0) {
    						$('.dialog_end .curtime').text(leftTime(cha));
    					} else {
    						clearInterval(ticks);
    					}
    					cha--;
    				}, 1000);
    				$('.dialog_end .curtime').text(leftTime(cha));
    			}
    			random=data.entry_game_popout.tips_type;
    			if(data.entry_game_popout.tips_type==2){
    				$('.dialog_price .adv img').attr('src',data.entry_game_popout.redpacket_img);
    				$('.dialog_end .adv img').attr('src',data.entry_game_popout.redpacket_img);
    				$('.dialog_price .price_con p').text(data.entry_game_popout.redpacket_money);
    				$('.dialog_price .ad_title').text(data.entry_game_popout.ad_title);
    			}else{
    				$('.dialog_success .ad_img').attr('src',data.entry_game_popout.ad_img);
    				$('.dialog_end .adv img').attr('src',data.entry_game_popout.ad_img);
    				$(".dialog_success .ad_title").text(data.entry_game_popout.ad_title);
    			}
    			if (!isClick) {
    				return false;
    			}
    			isClick = false;
    			clearInterval(tick);
          var this_X = that.offset().left + getWidth() / 375 * 15;      //喷壶位置X
          var this_Y = that.offset().top - getWidth() / 375 * 30;       //喷壶位置Y
          $(".watering_can").addClass("active").css({left: this_X, top: this_Y});
          i = 0;
          m = 0;
          wateringPlant();
          that.addClass("active");
          price_time--;
          $(".tips_box p strong").text(price_time);
          setTimeout(function () {
          	$(".ui-mask").fadeIn();
          	if (random==2) {
          		$(".dialog_price").show();
          	} else {
          		$(".dialog_success").show();
          	}
          	that.removeClass("active");
          	isClick = true;
          }, 1500);
          $('.tips_box strong').text(data.num);
      },
      error:function(){
      	tipError("数据请求失败，请检查当前网络");
      }
  });
    }
});
//喷壶喷水
function wateringPlant() {
	tick = setInterval(function () {
		if (m < 60 && i < 20) {
			$(".watering_can").css("background-position", -2.6 * i + "rem");
			i++;
			m++;
		} else if (m < 60 && i >= 20) {
			$(".watering_can").css("background-position", "0 0");
			i = 1;
			m++;
		} else {
			$(".watering_can").css("background-position", "0 0");
			$(".watering_can").css({left: "auto", top: "auto"}).removeClass("active");
			clearInterval(tick);
		}
	}, 30);
}
//浇花气泡
setInterval(function () {
	if (j > 6) {
		j = 1;
	}
	$(".water").removeClass("active");
	$(".draw_item:nth-child(" + j + ") .water").addClass("active");
	j++;
}, 1000);
//花叶摇动
setInterval(function () {
	$(".draw_item .flower_hb").each(function () {
		if ($(this).hasClass("active")) {
			$(this).removeClass("active");
		} else {
			$(this).addClass("active");
		}
	});
}, 500);

//飞出下个游戏的链接
setTimeout(function(){
	paramData={'uid':uid,'time':time,'auth':auth,'device':device,'g_id':g_id};
	paramData=JSON.stringify(paramData);
	paramData=encryptByDES(paramData,key_auth);
	$.ajax({
		type:water.next_appear.type,
		url:water.url,
		data:{action:water.next_appear.action,uid:uid,reqType:2,paramData:paramData},
		dataType:'json',
		success:function(data){
			console.log(data);
			if(data.status==1&&isNextgame){
				$('.nextgame').fadeIn();
				window.localStorage.setItem('nextShow','true');
			}
		},
		error:function(){
			tipError('数据请求失败，请检查当前网络');
		}
	});
},8000);
//跳转下个游戏链接
$('.nextgame').click(function(){
	paramData={'uid':uid,'time':time,'auth':auth,'device':device,'g_id':g_id};
	paramData=JSON.stringify(paramData);
	paramData=encryptByDES(paramData,key_auth);
	$.ajax({
		type:water.next_entry.type,
		url:water.url,
		data:{action:water.next_entry.action,uid:uid,reqType:2,paramData:paramData},
		dataType:'json',
		success:function(data){
			if(data.status==1){
				window.localStorage.removeItem('nextShow');
				window.location.href="/h5/game.answer/home.html";
			}
		},
		error:function(){
			tipError('数据请求失败，请检查当前网络');
		}
	});
});
