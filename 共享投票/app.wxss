.bgpic {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: -1;
}
.item-wrap,
.active-wrap {
  padding-bottom: 129rpx;
}
.loadmore {
  height: 20rpx;
  line-height: 20rpx;
  font-size: 20rpx;
  color: #ccc;
  text-align: center;
}
.share-img {
  position: fixed;
  bottom: 200rpx;
  right: 0rpx;
  width: 92rpx;
  height: 92rpx;
  z-index: 998;
}
.share-img image {
  width: 100%;
  height: 100%;
}
.corr-wrap {
  display: flex;
  align-items: center;
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  background-color: rgba(0,0,0,0.7);
  z-index: 999;
}
.corr-wrap .corr-wrap-ctt {
  width: 100%;
}
.corr-wrap .corr-wrap-ctt .corr-list {
  background-color: #fff;
  height: 526rpx;
  padding: 0 12rpx;
}
.corr-wrap .corr-wrap-ctt .corr-list .corr-list-item {
  display: inline-block;
  border-radius: 10rpx;
  overflow: hidden;
  box-shadow: 0 10rpx 10rpx -10rpx #000;
  width: 350rpx;
  margin-top: 24rpx;
}
.corr-wrap .corr-wrap-ctt .corr-list .corr-list-item:nth-child(2n) {
  margin-left: 24rpx;
}
.corr-wrap .corr-wrap-ctt .corr-list .corr-list-item .corr-img {
  width: 350rpx;
  height: 176rpx;
}
.corr-wrap .corr-wrap-ctt .corr-list .corr-list-item .corr-img image {
  width: 100%;
  height: 100%;
}
.corr-wrap .corr-wrap-ctt .corr-list .corr-list-item .corr-item-tit {
  width: 350rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  color: #333333;
  font-size: 22rpx;
  line-height: 28rpx;
}
.jishuzhichi {
  font-size: 28rpx;
  text-align: center;
}
.jishuzhichi text {
  font-size: 34rpx;
}