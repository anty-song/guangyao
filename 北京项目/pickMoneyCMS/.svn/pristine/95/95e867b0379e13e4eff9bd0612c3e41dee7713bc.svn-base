@media only screen and (max-width:750px){
    *{max-width: 750px;margin: auto;}
    html{font-size: calc(100/750*100vw);}
}
@media only screen and (min-width:750px){
    *{max-width: 750px;}
     html{font-size: 100px;margin: auto;}
}
html{max-width: 750px;}

#head-bar{background: #000000;}
.content .head-bar-fix {clear:both;}
.content .head-bar {width:100%;height:.96rem;line-height:.96rem;overflow:hidden;background:#F8F8F8;border-bottom:#B2B2B2 1px solid;position:relative;top:0;}
.content .head-bar img {width:.48rem;height:.48rem;vertical-align:top;padding-top:.24rem;}
.content .head-bar span {color:#007AFF;font-size:.32rem;}
.content .head-bar-title {position:absolute;z-index:9;left:1.6rem;right:1.6rem;font-size:.36rem;text-align:center;word-wrap:normal;text-overflow:ellipsis;white-space:nowrap;overflow:hidden;color:#333333;}
.content .head-bar-left {float:left;padding:0 0 0 .2rem;}
.content .head-bar-back {float:left;padding:0 0 0 .2rem;}
.content .head-bar-back a{text-decoration: none;}
.content .head-bar-right {float:right;padding:0 .2rem 0 0;}
.content .head-bar{background: white;}

.content{background: url(../images/WaterFlow/jhyx/jhyx_bg.png) no-repeat center 5.06rem/100% calc(100vh - 5.06rem);min-height: 100vh;position: relative;}
.nav{height: .88rem;line-height: .88rem;text-align: center;font-size: .36rem;color: #000;}
.nav i{float: left;width: .26rem;height: .46rem;background: url(../images/dialog/icon-back.png) 0 0 no-repeat;background-size: 100% 100%;margin-top: .21rem;margin-left: .2rem;}
.banner{height: 4.1rem;background:url(../images/WaterFlow/jhyx/jhyx_banner.png) no-repeat center/7.5rem 4.1rem;position: relative;}
.rules{width: 1rem;height: .34rem;position: absolute;left:.3rem;top:.4rem;background: url(../images/WaterFlow/jhyx/jhyx_rule.png) no-repeat center/100%;}
.my_price{width: .82rem;height: 1.06rem;position: absolute;top: 0;right: .2rem;background:url(../images/WaterFlow/jhyx/jhyx_wdjp.png) no-repeat center/100%;}
.notice{width: 4.98rem;height: .74rem;position: absolute;top: 3.58rem;left: 0;right: 0;margin: auto;padding: .2rem 0 .32rem;background: url(../images/WaterFlow/jhyx/jhyx_notice_bg.png) no-repeat center/100%;overflow: hidden;}
.notice_box{position: absolute;height: .74rem;width: 100%;overflow: hidden;}
.notice_list{position: absolute;margin: 0;padding: 0;width: 100%;height:1.26rem;left: 0;top: 0;font-size: 0;}
.notice_item{width: 100%;height:.74rem;display: inline-block;}
.notice p{line-height: .74rem;margin: 0;text-align: center;font-size: .26rem;color: #f56447;white-space: nowrap;}
.draw_box{position: absolute;top: 35%;}
.draw_list{font-size: 0;text-align: center;margin: 0;padding: 0;}
.draw_item{width: 1.9rem;height: 2rem;margin: .18rem;display: inline-block;background: url(../images/WaterFlow/jhyx/jhyx_flower.png) no-repeat center bottom/100%;position: relative;}
.draw_item .flower_hb{width: .93rem;height: .76rem;position: absolute;left: 0;right: 0;top: .06rem;background: url(../images/WaterFlow/jhyx/jhyx_flower_hb1.png) no-repeat center .06rem/.93rem .76rem;-webkit-transform: scale3d(1,1,1);transform: scale3d(1,1,1);-webkit-transition:-webkit-transform 1.2s ease-out .8s; transition:transform 1.2s ease-out .8s;}
.draw_item .flower_hb.active{background-image: url(../images/WaterFlow/jhyx/jhyx_flower_hb2.png);}
.draw_item.active .flower_hb{-webkit-transform: scale3d(1.5,1.5,1.5);transform: scale3d(1.5,1.5,1.5);}
.draw_item .water{position: absolute;width: .8rem;height: .8rem;top: -.5rem;right: .1rem;background: url(../images/WaterFlow/jhyx/jhyx_bubble.png) no-repeat center/100%;display: none;}
.tips_box{position: absolute;top: 70%;width: 100%;overflow: hidden;}
.draw_item .water.active{display: block;}
.tips_box p{margin: .26rem 0;text-align: center;font-size: .26rem;line-height: .36rem;color:#137f40;}
.price_box{height: 2.8rem;width: 100%;padding: .5rem .24rem 0 .29rem;position: absolute;bottom: 2%;background: url(../images/WaterFlow/jhyx/jhyx_jpzs_bg.png) no-repeat center .5rem/100% 2.3rem;box-sizing: border-box;overflow: hidden;}
.price_box h3{position: absolute;line-height: .36rem;width: 1.4rem;font-size: .24rem;color:#137f40;background:#fff600;border-radius:.18rem;text-align: center;font-weight: normal;margin: auto;left: 0;right: 0;top:.3rem;}
.price_wrap{width: 6.95rem;height: 100%;overflow-x: scroll;}
.price_list{padding:0 .28rem;font-size: 0;width: 18rem;max-width: unset;}
.price_item{display: inline-block;width:1.52rem;height: 1.7rem;margin: .3rem .13rem;background: url(../images/WaterFlow/jhyx/jhyx_jpzs_hb_bg.png) no-repeat center/100% 100%;}
/*.price_item a{display: block;width: 100%;height: 100%;background: url(../images/WaterFlow/jhyx/jhyx_hb.png) no-repeat center/.8rem .94rem;}*/
.price_item a{display: block;width: 100%;height: 100%;text-align: center;}
.price_item a img{width: 80%; height: 80%;margin-top: 10%;}
.watering_can{width: 2.6rem;height: 1.4rem;position: absolute;margin: 0;bottom:18%;right: .2rem;background:url(../images/WaterFlow/jhyx/jhyx_ph.png) no-repeat 0 0/52rem 2.1rem;}
.watering_can.active{width:2.6rem;height: 2.1rem;background-image: url(../images/WaterFlow/jhyx/jhyx_ph.png);background-repeat:no-repeat;background-position:0;background-size: 52rem 2.1rem;}

/*蒙版*/
#refuse{position: absolute;bottom: .8rem;left: 26%;width: 1.7rem;height: .5rem;border-radius: .25rem;
        background: #FFF288;border: none;outline: none;color: #F18A74;background-size: 100% 100%;}
#againBtn{position: absolute;bottom: .8rem;right: 26%;width: 1.7rem;height: 0.5rem;border-radius: .25rem;outline: none;border: none;color: #F92A30;background: url(../images/dialog/continue.png) 0 0 no-repeat;background-size: 100% 100%;}
.nextgame{position: absolute;  left: 2%;  bottom: 2.6rem;  width:15%;  display: none;}
