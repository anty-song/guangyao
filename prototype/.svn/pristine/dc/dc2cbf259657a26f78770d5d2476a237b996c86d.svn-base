body {
    background: #F8F9FB;
}
/*后台列表主体*/
.oaMain {
    width: 12rem;
    height: auto;
    margin: 0 auto;
    padding-top: 0.6rem;
}
/*功能项*/
.oaMain>.oaMain_fun {
    width: 100%;
    height: 0.74rem;
    overflow: hidden;
}
.oaMain>.oaMain_fun>li {
    float: left;
    width: 1rem;
    height: 0.3rem;
    margin: 0.22rem 0.38rem 0.22rem 0;
}
.oaMain>.oaMain_fun>li>a {
    display: block;
    color: #8fc7fc;
    font-size: 0.2rem;
    line-height: 0.3rem;
}
.oaMain>.oaMain_fun>li:nth-child(4n)>a,.oaMain>.oaMain_fun>li:first-child>a {
    color: #0084FF;
}

.oaMain>.oaMain_fun>li>a>img {
    margin: 3px 8px;
    float: left;
}

/*前进后退与搜索的菜单栏*/
.oaMain>.oaMain_menu {
    width: 100%;
    height: 0.7rem;
    background: #fff;
    margin-bottom: 0.24rem;
    border-radius: 6px;
    box-shadow:0 6px 4px rgba(231,231,239,0.2);
}
.oaMain>.oaMain_menu>a {
    display: block;
    width: 0.23rem;
    height: 0.23rem;
    overflow: hidden;
    float: left;
    margin: 0.23rem 0.12rem 0.23rem 0.2rem;
}
.oaMain>.oaMain_menu>a#gosBtn {
    margin-left: 0;
    margin-right: 0.2rem;
}
.oaMain>.oaMain_menu>a>img {
    width: 100%;
    height: 100%;
}
/*分割线*/
.oaMain>.oaMain_menu>b {
    display: block;
    float: left;
    width: 2px;
    height: 0.32rem;
    margin: 0.19rem 0.28rem 0.19rem 0.13rem;
    background: #333;
}
.oaMain>.oaMain_menu>p {
    float: left;
    overflow: hidden;
}
.oaMain>.oaMain_menu>p>a {
    display: block;
    float: left;
    font-size: 0.18rem;
    line-height: 0.7rem;
    color: #333;
}
.oaMain>.oaMain_menu>p>img {
    display: block;
    float: left;
    width: 0.18rem;
    height: 0.18rem;
    height: 0.18rem;
    margin: 0.26rem 0.05rem;
}

/*搜索*/
.oaMain>.oaMain_menu>.oaMain_menu_search {
    float: right;
    width: 2.1rem;
    height: 0.4rem;
    overflow: hidden;
    margin: 0.15rem 0.2rem;
    background: #F8F9FB;
    border-radius: 0.21rem;
}
.oaMain>.oaMain_menu>.oaMain_menu_search>img {
    width: 0.21rem;
    float: left;
    height: 0.25rem;
    margin: 8px 0.1rem 0 0.14rem;
}
.oaMain>.oaMain_menu>.oaMain_menu_search form input {
    width: 1.3rem;
    height: 0.4rem;
    font-size: 0.16rem;
    color: #666;
    background: #F8F9FB;
    line-height: 0.4rem;
}

/*表头*/
.oaMain>.oaMain_title {
    width: 100%;
    height: 0.62rem;
    background: #EBF3FB;
    border-radius: 6px 6px 0 0;
}
/*表头全选*/
.oaMain>.oaMain_title>p {
    float: left;
    width: 2.5rem;
    height: 0.62rem;
    font-size: 0.2rem;
    line-height: 0.62rem;
    color: #0A69C1;
}
.oaMain>.oaMain_title>p>label {
    display: block;
    float: left;
    width: 0.4rem;
    font-weight: bold;
    cursor: pointer;
}
.oaMain>.oaMain_title>p>label>i {
    display: block;
    float: left;
    font-size: 0.24rem;
    color: #0084FF;
    line-height: 0.62rem;
    margin: 0 0.06rem 0 0.1rem;
}
.oaMain>.oaMain_title>p>label>input[type=checkbox] {
    display: none;
}
/*表头链接*/
.oaMain>.oaMain_title>em {
    display: block;
    float: left;
    width: 6rem;
    height: 100%;
    font-size: 0.2rem;
    line-height: 0.62rem;
    font-weight: bold;
    color: #0A69C1;
    text-align: center;
}
/*表头密码*/
.oaMain>.oaMain_title>b {
    display: block;
    float: left;
    width: 1.1rem;
    height: 100%;
    font-size: 0.2rem;
    line-height: 0.62rem;
    font-weight: bold;
    color: #0A69C1;
    text-align: center;
}
/*表头时间*/
.oaMain>.oaMain_title>span {
    width: 2.4rem;
    height: 100%;
    float: left;
    display: block;
}
.oaMain>.oaMain_title>span>i {
    display: block;
    float: left;
    margin-left: 0.68rem;
}
.oaMain>.oaMain_title>span>i>a {
    color: #0A69C1;
    font-size: 0.2rem;
    line-height: 0.62rem;
    font-weight: bold;
}
.oaMain>.oaMain_title>span>img {
    width: 0.23rem;
    height: 0.23rem;
    float: right;
    margin-top: 0.2rem;
    margin-right: 0.62rem;
}

/*内容*/
.oaMain_list {
    width: 100%;
    box-shadow:0rem 6px 4px rgba(231,231,239,0.2);
    border-radius:  0 0 6px 6px;
    background: #fff;
}
.oaMain_list>.oaMainBox {
    width: 100%;
    height: 0.53rem;
    margin-top: 0.04rem;
    border-top: 1px solid #f6f7f9;
    border-radius:6px;
    background: #fff;
}
.oaMain_list>.oaMainBox.on {
    background: #EBF3FB;

}
/*内容选择框*/
.oaMain_list>.oaMainBox>label {
    display: block;
    float: left;
    height: 100%;
    cursor: pointer;
}
.oaMain_list>.oaMainBox>label>i {
    display: block;
    float: left;
    font-size: 0.2rem;
    line-height: 0.57rem;
    margin: 0 0.1rem 0 0.1rem;
    color: #0084FF;
}
.oaMain_list>.oaMainBox>label>input {
    display: none;
}
.oaMain_list>.oaMainBox>span {
    display: block;
    float: left;
    overflow: hidden;
    width: 2.1rem;
    height: 0.57rem;
    overflow: hidden;
}
.oaMain_list>.oaMainBox>span>img {
    float: left;
    width: 0.2rem;
    height: auto;
    margin-top: 0.21rem;
    margin-right: 8px;
}
.oaMain_list>.oaMainBox>span>a {
    float: left;
    font-size: 0.16rem;
    line-height: 0.57rem;
    color: #333;
}
/*内容地址*/
.oaMain_list>.oaMainBox>em {
    width: 6rem;
    float: left;
    height: 0.57rem;
    display: block;
    font-size: 0.16rem;
    text-align: left;
    overflow: hidden;
    position: relative;
}
.oaMain_list>.oaMainBox>em>a {
    display: inline-block;
    line-height: 0.57rem;
    color: #0084FF;
    float: left;
}
.oaMain_list>.oaMainBox>em>a>input {
    display: block;
    opacity: 0;
    width: 1px;
    height: 1px;
}
.oaMain_list>.oaMainBox>em>a:hover {
    text-shadow:0rem 1px 3px rgba(113,168,219,1);
    font-weight: bold;
    margin-left: 0;
}
.oaMain_list>.oaMainBox>em>a:hover b {
    display: block;
    text-shadow:none;
}
.oaMain_list>.oaMainBox>em>a>b {
    display: none;
    position: absolute;
    width: 0.4rem;
    height: 0.22rem;
    font-size: 0.14rem;
    line-height: 0.22rem;
    color: #fff;
    text-align: center;
    background: #ff8c64;
    border-radius: 4px;
    top: 0.18rem;
    right: 0;
}

/*内容密码*/
.oaMain_list>.oaMainBox>b {
    display: block;
    float: left;
    width: 1.1rem;
    height: 0.57rem;
    overflow: hidden;
    font-size: 0.16rem;
    color: #333;
    text-align: center;
    line-height: 0.57rem;
}
/*内容时间*/
.oaMain_list>.oaMainBox>i {
    display: block;
    float: left;
    width: 2.4rem;
    height: 0.57rem;
    font-size: 0.16rem;
    line-height: 0.57rem;
    color: #333;
    text-align: center;
}
/*鼠标滑过box样式*/
.oaMain>.oaMain_list>.oaMainBox:hover {
    box-shadow: 0.01rem 0.04rem 0.04rem rgba(0,132,255,0.2),
    -0.01rem -0.01rem 0.01rem rgba(0,132,255,0.2);
}


/*删除窗口*/
.oaMain_del,.oaMain_move,.oaMain_uploadinging {
    display: none;
    width: 100%;
    height: 100%;
    background: rgba(100,100,100,0.2);
    top: 0;
    left: 0;
    position: absolute;
}
/*删除窗口*/
.oaMain_del>.del_window {
    width: 4.72rem;
    height: 1.82rem;
    border-radius: 8px;
    background: #fff;
    margin: 3rem auto 0;
}
/*删除窗口上*/
.oaMain_del>.del_window>p {
    padding: 5px 0.17rem 0 0.17rem;
    height: 0.4rem;
    font-size: 0.16rem;
    line-height: 0.4rem;
    color: #333;
}
.oaMain_del>.del_window>p>a {
    display: block;
    float: right;
    width: 0.24rem;
    height: 0.24rem;
    font-size: 0.18rem;
    line-height: 0.24rem;
    margin-top: 8px;
    text-align: center;
}
/*删除窗口中*/
.oaMain_del>.del_window>span {
    display: block;
    width: 100%;
    height: 0.85rem;
    border-top: 1px solid #eee;
    border-bottom: 1px solid #eee;
    font-size: 0.2rem;
    line-height: 0.85rem;
    color: #333;
}
.oaMain_del>.del_window>span>i {
    display: block;
    width: 0.27rem;
    height: 0.27rem;
    border-radius: 50%;
    background: #FF8C64;
    font-size: 0.2rem;
    line-height: 0.27rem;
    margin: 0.29rem 8px 0 0.25rem;
    text-align: center;
    color: #fff;
    float: left;
}
/*删除窗口下按钮*/
.oaMain_del>.del_window>button {
    display: block;
    width: 0.52rem;
    height: 0.3rem;
    font-size: 0.18rem;
    line-height: 0.32rem;
    color: #333;
    float: right;
    background: #e6e6e6;
    margin-right: 0.22rem;
    border-radius: 4px;
    margin-top: 0.1rem;
}
.oaMain_del>.del_window>button#delYesBtn {
    background: #0084FF;
    color: #fff;
}

/*移动到窗口*/
.oaMain_move {
    /*display: block;*/
}
.oaMain_move>.move_window,.oaMain_uploadinging>.move_window{
    width: 4.37rem;
    height: 3.15rem;
    background: #fff;
    border-radius: 8px;
    margin: 2.9rem auto 0;
}
.oaMain_move>.move_window>p,.oaMain_uploadinging>.move_window>p {
    font-size: 0.16rem;
    color: #333;
    line-height: 0.44rem;
    padding: 0 0.17rem;
}
.oaMain_move>.move_window>p>a,.oaMain_uploadinging>.move_window>p>a {
    display: block;
    float: right;
    width: 0.24rem;
    height: 0.24rem;
    font-size: 0.18rem;
    line-height: 0.24rem;
    text-align: center;
    margin-top: 0.1rem;
}
/*移动主体*/
.oaMain_move>.move_window>.moveMain,.oaMain_uploadinging>.move_window>.moveMain {
    width: 100%;
    height: 2.18rem;
    overflow: auto;
    border-top: 1px solid #eee;
    border-bottom: 1px solid #eee;
}
/*1级*/
.oaMain_move>.move_window>.moveMain ul,.oaMain_uploadinging>.move_window>.moveMain ul {
    padding-left: 0.2rem;
    overflow: auto;
}
.oaMain_move>.move_window>.moveMain ul>li,.oaMain_uploadinging>.move_window>.moveMain ul>li {
    width: 100%;
    height: 0.4rem;
    overflow: hidden;
}
.oaMain_move>.move_window>.moveMain ul>li.on,.oaMain_uploadinging>.move_window>.moveMain ul>li.on {
    border-bottom: 1px solid #eee;
}
.oaMain_move>.move_window>.moveMain ul>li.on>a>span, .oaMain_uploadinging>.move_window>.moveMain ul>li.on>a>span{
    color: #0084ff;
    font-weight: 600;
}
.oaMain_move>.move_window>.moveMain ul>li>a,.oaMain_uploadinging>.move_window>.moveMain ul>li>a {
    display: inline-block;
    cursor: pointer;
    height: auto;
    width: 100%;
    overflow: auto;
}
.oaMain_move>.move_window>.moveMain ul>li>a:hover,.oaMain_uploadinging>.move_window>.moveMain ul>li>a:hover {
    color: #0084ff;
}
.oaMain_move>.move_window>.moveMain ul>li>a>img,.oaMain_uploadinging>.move_window>.moveMain ul>li>a>img {
    width: 0.2rem;
    height: 0.16rem;
    float: left;
    margin-top: 0.12rem;
    margin-right: 0.1rem;
}
.oaMain_move>.move_window>.moveMain ul>li>a>span,.oaMain_uploadinging>.move_window>.moveMain ul>li>a>span {
    display: inline-block;
    float: left;
    font-size: 0.16rem;
    line-height: 0.4rem;
    margin-right: 5px
}


/*移动按钮选项*/
.oaMain_move>.move_window>button,.oaMain_uploadinging>.move_window>button {
    display: block;
    width: 0.52rem;
    height: 0.3rem;
    background: #0084FF;
    color: #fff;
    font-size: 0.18rem;
    line-height: 0.3rem;
    float: right;
    margin-top: 0.1rem;
    border-radius:4px;
    margin-right: 0.2rem;
    cursor: pointer;
}
.oaMain_move>.move_window>button#moveAddBtn,.oaMain_uploadinging>.move_window>button#moveAddBtn {
    float: left;
    margin-left: 0.2rem;
}
.oaMain_move>.move_window>button#moveNoBtn,.oaMain_uploadinging>.move_window>button#moveNoBtn {
    background: #e6e6e6;
    color: #333;
}
