var ticks,                                    //倒计时定时器
cha,                                          //倒计时时间
uid,                                          //用户ID
auth,                                         //用户权重
key_auth,                                     //用户解码
device,                                       //用户设备
p_id,                                         //红包ID
ad_url,                                       //广告链接
time=Math.round(new Date() / 1000),           //请求时间戳
g_id=4,                                       //当前游戏id
paramData,                                    //定义传递数据对象
isNextgame=false,                             //下个游戏弹窗是否飞出
thisgame_time,                                //当前游戏次数（固定次数+再来一次）
no_times_url='/h5/game.game_lobby/no_times',  //游戏次数用完
tick,                                         //喷水定时器
i = 0,                                        //喷水单次步进
j = 1,                                        //文字泡索引
m = 0,                                        //喷水总步进
price_time=parseInt($(".tips_box p strong").text()),         //抽奖剩余次数
isClick = true,                               //是否允许浇花
isAgain=false;                                //是否是再来一次

var water = new waterConfig();                //实例化配置
window.onload=function(){
	if(window.localStorage.getItem('nextShow')&&isNextgame){
		$('.nextgame').fadeIn();
	}
	uid=water.uid;
	auth=water.auth;
	device=water.device;
	if(!window.localStorage.getItem('water_alert')){
		window.localStorage.setItem('water_alert','2');
	}
	paramData={'uid':uid,'time':time,'auth':auth,'device':device,'game_type':g_id};
	key_auth=auth.substring(0,8);
	paramData=JSON.stringify(paramData);
	paramData=encryptByDES(paramData,key_auth);
	if(auth.length<16){
		return false;
	}
	if(window.localStorage.getItem('water_alert') =='2'){
		$.ajax({
			type:water.load.type,
			url:water.url,
			data:{action:water.load.action,uid:uid,reqType:2,paramData:paramData},
			dataType:'json',
			success:function(data){
				var data=decryptByDES(data.res_data, key_auth);
				data=JSON.parse(data);
				console.log(data);
				cha=data.count_down;
				$('.tips_box p strong').text(data.num);
				p_id=data.p_id;
				ad_url=data.entry_game_popout.ad_url;
				thisgame_time=(data.game_times[4].once_count!=0||data.game_again_times[4].again_count!=0)?1:2;
				if(thisgame_time==2){
					window.location.href=no_times_url;
				}
				if(data.num==0){
					$('.dialog_end .curtime').text(leftTime(cha));
					ticks = setInterval(function () {
						if (cha > 0) {
							$('.dialog_end .curtime').text(leftTime(cha));
						} else {
							clearInterval(ticks);
							window.localStorage.setItem('water_alert',2);
							window.location.reload();
						}
						cha--;
					}, 1000);
					$(".dialog_end").show();
				}else{
					if(data.entry_game_popout.tips_type==2){
						$('.dialog_price .adv img').attr('src',data.entry_game_popout.redpacket_img);
						$('.dialog_end .adv img').attr('src',data.entry_game_popout.redpacket_img);
						$('.dialog_price .price_con p').text(data.entry_game_popout.redpacket_money);
						$('.dialog_price .ad_title').text(data.entry_game_popout.ad_title);
						if(window.localStorage.getItem('water_alert')!=='1'){
							$(".dialog_price").show();
						}
					}else{
						$('.dialog_success .ad_img').attr('src',data.entry_game_popout.ad_img);
						$('.dialog_end .adv img').attr('src',data.entry_game_popout.ad_img);
						$(".dialog_success .ad_title").text(data.entry_game_popout.ad_title);
						if(window.localStorage.getItem('water_alert')!=='1'){
							$(".dialog_success ").show();
						}
					}
				}
				$(".ui-mask").fadeIn();
				if(data.game_times[1].once_count!=0||data.game_again_times[1].again_count!=0){
					isNextgame=true;
				}else{
					isNextgame=false;
				}
			},
			error:function(){
				tipError("数据请求失败，请检查当前网络");
			}
		});
	}else{
		$.ajax({
			type:water.refresh.type,
			url:water.url,
			data:{action:water.refresh.action,uid:uid,reqType:2,paramData:paramData},
			dataType:'json',
			success:function(data){
				console.log(data);
				var data=decryptByDES(data.res_data, key_auth);
				data=JSON.parse(data);
				console.log(data);
				thisgame_time=(data.game_times[4].once_count!=0||data.game_again_times[4].again_count!=0)?1:2;
				if(thisgame_time==2){
					window.location.href=no_times_url;
				}
				cha=data.count_down;
				$('.tips_box p strong').text(data.num);
				if(agent.isAndroid && agent.isPhone) {
					ad_url='http://www.zsyouxue.com/app/appversion/zhangshangyouxue_studyshow_1.0.5.apk';
				} else if(agent.isIOS && agent.isPhone) {
					ad_url="https://itunes.apple.com/cn/app/%E6%8E%8C%E4%B8%8A%E4%BC%98%E5%AD%A6/id1382476478?mt=8";
				}
				if(data.count_down!=0){
					$('.dialog_end .curtime').text(leftTime(cha));
					if(data.num==0){
						$(".dialog_end").show();
						$(".ui-mask").fadeIn();
					}
					clearInterval(ticks);
					ticks = setInterval(function () {
						if (cha > 0) {
							$('.dialog_end .curtime').text(leftTime(cha));
						} else {
							clearInterval(ticks);
							window.localStorage.setItem('water_alert',2);
							window.location.reload();
						}
						cha--;
					}, 1000);
				}
				if(data.game_times[1].once_count!=0||data.game_again_times[1].again_count!=0){
					isNextgame=true;
				}else{
					isNextgame=false;
				}
			},
			error:function(){
				tipError("数据请求失败，请检查当前网络");
			}
		});
	}
};
