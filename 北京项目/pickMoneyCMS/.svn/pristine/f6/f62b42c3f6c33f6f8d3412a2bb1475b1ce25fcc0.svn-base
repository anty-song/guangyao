@media only screen and (max-width:750px){
    *{max-width: 750px;margin: auto;}
    html{font-size: calc(100/750*100vw);}
}
@media only screen and (min-width:750px){
    *{max-width: 750px;}
     html{font-size: 100px;margin: auto;}
}
html{max-width: 750px;}

#head-bar{background: #000000;}
.content .head-bar-fix {clear:both;}
.content .head-bar {width:100%;height:.96rem;line-height:.96rem;overflow:hidden;background:#F8F8F8;border-bottom:#B2B2B2 1px solid;position:relative;top:0;}
.content .head-bar img {width:.48rem;height:.48rem;vertical-align:top;padding-top:.24rem;}
.content .head-bar span {color:#007AFF;font-size:.32rem;}
.content .head-bar-title {position:absolute;z-index:9;left:1.6rem;right:1.6rem;font-size:.36rem;text-align:center;word-wrap:normal;text-overflow:ellipsis;white-space:nowrap;overflow:hidden;color:#333333;}
.content .head-bar-left {float:left;padding:0 0 0 .2rem;}
.content .head-bar-back {float:left;padding:0 0 0 .2rem;}
.content .head-bar-back a{text-decoration: none;}
.content .head-bar-right {float:right;padding:0 .2rem 0 0;}
.content .head-bar{background: white;}

.content{background: url(../images/GameLobby/untimes_bg.png) no-repeat center center/100% 100%;min-height: 100vh;position: relative;}
.content .btn-box{position: absolute;top:72%;width: 100%;}
.content .btn-box button{display: block;width: 5.06rem;height: 1.14rem;margin: auto;line-height: 1rem;text-align: center;font-size: .46rem;color: #FFFFFF;background: url(../images/GameLobby/untimes_btn.png) no-repeat center center/100% 100%;border:none;outline: none;}
