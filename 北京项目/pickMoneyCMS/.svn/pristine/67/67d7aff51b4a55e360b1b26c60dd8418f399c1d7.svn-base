#head-bar{background: #000000;}
.head-bar-fix {
	/*height:48px;*/
	clear:both;
}
.head-bar {width:100%;height:48px;line-height:48px;overflow:hidden;background:#F8F8F8;border-bottom:#B2B2B2 1px solid;position:relative;top:0;z-index: 99999;}
.head-bar img {width:24px;height:24px;vertical-align:top;padding-top:12px;}
.head-bar span {color:#007AFF;font-size:16px;}
.head-bar-title {position:absolute;z-index:9;left:80px;right:80px;font-size:18px;text-align:center;word-wrap:normal;text-overflow:ellipsis;white-space:nowrap;overflow:hidden;color:#333333;}
.head-bar-left {float:left;padding:0 0 0 10px;}
.head-bar-back {float:left;padding:0 0 0 10px;}
.head-bar-back a{text-decoration: none;}
.head-bar-right {float:right;padding:0 10px 0 0;}
.head-bar{
	background: white;
}
*{
    -webkit-tap-highlight-color: transparent;
    -webkit-touch-callout: none;
    -webkit-user-select: none;
}

.ui-mask{
    position: fixed;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,.85);
    z-index: 100;
    left: 0;
    top: 0;
    display: none;
}

.dialog{
    /*display: none;*/
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    margin: auto;
    z-index: 9999;
}

.dialog_price{
    width: 7.5rem;
    height: 8.4rem;
    display: none;
}

.dialog_price .dialog_body{
    width: 100%;
    height: 100%;
    background: url("../images/dialog/price_bg.png") no-repeat;
    -webkit-background-size: contain;
    background-size: 100% 100%;
    background-position: center;
    position: absolute;
    top: 0rem;
}

.dialog_price.dialog .close{
    width: 0.6rem;
    height: 0.6rem;
    background: url("../images/dialog/close_w.png") no-repeat;
    -webkit-background-size: contain;
    background-size: contain;
    background-position: center center;
    position: absolute;
    right: 0.4rem;
    top: .2rem;
}

.dialog .dialog_header{
    width: 7.5rem;
    position: absolute;
    top: -1.2rem;
    left: 0;
    -webkit-animation:rotated 5s linear infinite;
    -moz-animation:rotated 5s linear infinite;
    animation:rotated 5s linear infinite;
    z-index: -1;
}

.dialog_wallet .dialog_header {
	top: -0.5rem;
}

.dialog_box .dialog_header{
	top: -1.5rem;
}

.dialog .dialog_header img{
    width: 100%;
}

.dialog.dialog_price .adv{
    width: 5.2rem;
    height: 3.2rem;
    background: rgb(255,110,77);
    box-shadow:0px 0px 0px 0px rgba(185,18,15,0.47)
    -webkit-border-radius: 0.1rem;
    -moz-border-radius: 0.1rem;
    border-radius: 0.1rem;
    margin: auto;
    margin-top: 1.8rem;
    position: relative;
    z-index: 1;
}

.dialog.dialog_price .adv img{
	width: 100%;
	height: 100%;
}

.dialog.dialog_price .adv_bottom{
    width: 5.2rem;
    height: 0.5rem;
    background: rgba(255,255,255,1);
    box-shadow:0px 0px 0px 0px rgba(185,18,15,0.47);
    margin-top: .1rem; 
    border-radius: .1rem;
}

.dialog.dialog_price .show{
    margin-top: 0.35rem;
    text-align: center;
    position: absolute;
    z-index: 1000;
    width: 100%;
}

.dialog.dialog_price .show .title{
    color: #FFFFFF;
    font-size: .26rem;
    text-align: center;
    height: .36rem;
    line-height: .48rem;
}
.ad_title{font-size: 0.24rem;
    text-align: center;
    line-height: .5rem;
}
.dialog.dialog_price .show .price_con{
    width: 2.93rem;
    height: 0.5rem;
    background: url(../images/dialog/price_money.png) no-repeat center/100%;
    font-size: .24rem;
    color: #FFFFFF;
    text-align: center;
    line-height: 0.5rem;
    margin: 0.25rem auto .2rem;
}

.dialog.dialog_price .getBtn{
    width: 1.78rem;
    height: 1.78rem;
    background: url("../images/dialog/price_get.png") no-repeat;
    -webkit-background-size: contain;
    background-size: contain;
    margin: auto;
    outline: none;
    border: none;
}

.dialog.dialog_price .close{
    z-index: 10000;
}

/*旋转动画*/
@keyframes rotated {
    from{
        transform: rotate(0deg);
        -webkit-transform: rotate(0deg);
    }
    to{
        transform: rotate(360deg);
        -webkit-transform: rotate(360deg);
    }
}

@-webkit-keyframes rotated {
    from{
       transform: rotate(0deg);
       -webkit-transform: rotate(0deg);
   }
   to{
     transform: rotate(360deg);
     -webkit-transform: rotate(360deg);
 }
}
/*成功弹出框*/
.dialog.dialog_success{
    width: 7.5rem;
    height: 8.1rem;
    text-align: center;
    display: none;
}

.dialog.dialog_success .dialog_body{
    width: 7.5rem;
    height: 8.4rem;
    background: url("../images/dialog/success_bg.png") no-repeat center -.3rem/100% 8.1rem;
    margin: auto;
    overflow: hidden;
}

.dialog.dialog_success .dialog_body .show{
    width: 5.2rem;
    height: 3.2rem;
    background: rgb(255,244,209);
    margin: 1.8rem auto 0;
    position: relative;
    overflow: hidden;
}
.dialog.dialog_success .dialog_body .show img{
	width: 100%;
	height: 100%;
}

.dialog.dialog_success .dialog_body .adv_bottom{
    line-height: 0.5rem;
    width: 5.2rem;
    height: 0.5rem;
    background: rgb(250,203,163);
    color: #FFFFFF;
    box-shadow:0px 0px 0px 0px rgba(185,18,15,0.47);
    font-size: 0.24rem;
    text-align: center;
    border-radius: 0.1rem;
    margin: .1rem auto .3rem;

}

.dialog.dialog_success .dialog_body .successBtn{
    width: 1.78rem;
    height: 1.78rem;
    background: url("../images/dialog/price_get.png") no-repeat center center/100% 100%;
    border: none;
    outline: none;
}

.dialog_success.dialog .close{
    width: 0.6rem;
    height: 0.6rem;
    background: url("../images/dialog/close_w.png") no-repeat;
    -webkit-background-size: contain;
    background-size: contain;
    background-position: center center;
    position: absolute;
    right: 0.4rem;
}

/*再玩一次弹出框*/
.dialog_again{
    width: 7.5rem;
    height: 7.5rem;
    text-align: center;
    top: 48px;
    left: 0rem;
    transform: translate(0rem,0rem);
    display: none;
    margin-top: 0rem;
}

.dialog_again .close{
    width: 0.6rem;
    height: 0.6rem;
    background: url("../images/dialog/close_g.png") no-repeat;
    -webkit-background-size: contain;
    background-size: contain;
    background-position: center center;
    position: absolute;
    right: 0.6rem;
    top: 0.9rem;
    z-index: 10;
}

.dialog_again .dialog_body{
    width: 7.5rem;
    height: 7.5rem;
    background: url("../images/dialog/dialog_again.png") no-repeat;
    background-position: center;
    -webkit-background-size: contain;
    background-size: contain;

}

.dialog_again .dialog_body .leaveBtn{
    width: 1.8rem;
    height: 0.53rem;
    background: #ffe938;
    border: none;
    outline: none;
    margin-top: 6.2rem;
    border-radius: 0.3rem;
    opacity: 0.6;
    color: #F92A30;
}

.dialog_again .dialog_body .continueBtn{
	width: 1.8rem;
    height: 0.53rem;
    background: url(../images/dialog/continue.png) no-repeat;
    background-position: center;
    background-size: contain;
    border: none;
    outline: none;
    color: #F92A30;
    margin-top: 6.2rem;
    border-radius: 0.3rem;
}
/*次数用完弹出框*/
.dialog_end{
    width: 7.5rem;
    height: 7.5rem;
    display: none;
    text-align: center;
    /*margin-top: 1.5rem;*/
    /*top: -30%;*/
}

.dialog_end .close{
    width: 0.6rem;
    height: 0.6rem;
    background: url("../images/dialog/close_g.png") no-repeat;
    -webkit-background-size: contain;
    background-size: contain;
    background-position: center center;
    position: absolute;
    right: 0.6rem;
    top: -0.1rem;
}

.dialog_end .dialog_body {
    width: 7.5rem;
    height: 7.5rem;
    background: url("../images/dialog/dialog_end.png") no-repeat;
    background-position: center;
    -webkit-background-size: contain;
    background-size: contain;
    overflow: hidden;
}

.dialog_end .dialog_body .bag{
    width: 7.5rem;
    height: 5.5rem;
    background: url('../images/dialog/dialog_end_bottom.png') no-repeat center 0/6.3rem 3.9rem;
    position: absolute;
    z-index: 500;
    left: 0;
    right: 0;
    top: 4.8rem;
}
.dialog_end .dialog_body .bag .bag_title:first-of-type{margin-top: .4rem !important;}
.dialog_end .dialog_body .bag .bag_title{
    color: #FFFFFF;
    font-size: 0.34rem;
    line-height: 0.54rem;
}

.dialog_end .dialog_body .bag .time{
    width: 3rem;
    margin: auto;
    height: 0.4rem;
    line-height: 0.4rem;
    background: rgb(255,146,153);
    border-radius: 0.2rem;
    font-size: 0.26rem;
    color: rgb(197,36,36);
    margin-top: 0.4rem;
}

.dialog_end .dialog_body .result{
    width: 5.2rem;
    height: 4.3rem;
    background: url("../images/dialog/dialog_end_show.png") no-repeat center .8rem/100% 100%;
    position: absolute;
    z-index: 200;
    left: 0;
    right: 0;
    /*top: -0.5rem;*/
    margin: auto;
    padding: 0.2rem;
    animation: open;
    animation-delay: 0.75s;
    animation-duration: 0.5s;
    animation-timing-function: ease-in;
    animation-fill-mode: both;
    /*animation: open 0.5s ease-in 0.75s 1;*/
}

.dialog_end .dialog_body .result .adv{
    width: 5.2rem;
    height: 3.2rem;
    background: #fff0d1;
    margin-top: .8rem;
}

.dialog_end .ad_title{color:#FF1200;}
.dialog_end .dialog_body .bag .endBtn{
    width: 3.56rem;
    height: 1.56rem;
    background: url("../images/dialog/play_again.png") no-repeat center/100%;
    border: none;
    outline: none;
    margin-top: 0.5rem;
    font-size:.48rem;
    font-weight:bold;
    color:rgba(252,82,44,1);
