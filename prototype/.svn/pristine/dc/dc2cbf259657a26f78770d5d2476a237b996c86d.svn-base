.dn{display: none!important;}
.db{display: block!important;}
.header{
    height: .6rem;
    line-height: .6rem;
    border-bottom: 1px solid #e7e7e7;
    position: fixed;
    width: 100%;
    background: #fff;
    z-index: 1000;
}
.header .topItem{
    max-width: 1200px;
    /*display: flex;*/
    /*justify-content: space-between;*/
    font-size: 0;
    margin: 0 auto;
    height: .6rem;
    line-height: .6rem;
    position: relative;
}
.header>.topItem>.topItem>p>a>img{
    width: 1.95rem;
    height: .2rem;
    cursor: pointer;
}
.header>.topItem>p {
    float: left;
}
.header>.topItem>p>a{
    display: block;
    float: left;
    font-weight: 400;
    color: #333;
    font-size: .18rem;
    margin-left: .15rem;
}
.header>.topItem>ul {
    float: right;
    font-size: 0.18rem;
    line-height: 0.6rem;
    color: #333;
}
.header>.topItem>ul>li {
    float: left;
    margin-left: 0.2rem;
}
.header>.topItem>ul>li>a {
    display: block;
}
.header>.topItem>ul>li:last-child {
    padding-right: 0.14rem;
    background: url("/static/style/prototype/images/adminup.png") right center no-repeat;
}
.header>.topItem>ul>li>a>img {
    width: 0.26rem;
    height: 0.26rem;
    margin-right: 0.05rem;
}
.header>.topItem>ul>li>a:hover,.header>.topItem>ul>li.on>a {
    color: #0084ff;
    background: url("/static/style/prototype/images/navBottom.png") no-repeat center bottom;
}
.header>.topItem>ul>li:last-child>a:hover {
    background: none;
}

.header>.topItem>span {
    display: none;
    width: 1.06rem;
    height: 0.72rem;
    overflow: hidden;
    border: 1px solid #e5e5e5;
    position: absolute;
    right: 0;
    top: 0.46rem;
    background: #fff;
}
.header>.topItem>span>a {
    display: block;
    margin: 0 0.08rem;
    font-size: 0.14rem;
    height: 0.36rem;
    line-height: 0.36rem;
    color: #333;
    border-top: 1px solid #e5e5e5;
}
.header>.topItem>span>a:hover {
    color: #0084ff;
}
.header>.topItem>span>a:first-child {
    border: none;
}
.header>.topItem>span>a>img {
    width: 0.15rem;
    height: 0.15rem;
    overflow: hidden;
    margin-right: 0.1rem;
}
.header p span{
    color: #999;
    font-size: .16rem;
    font-weight: 400;
    margin-left: .15rem;
    cursor: pointer;
}

.uploadBox {
    width: 12rem;
    overflow: hidden;
    margin: 0 auto;
    border-top: .6rem solid transparent;
    padding-top: .6rem;
}
.uploadBox>.liuchentu {
    width: 3.14rem;
    float: right;
    height: 5.76rem;
    border: 1px solid #d9d9d9;
    margin-right: 1.8rem;
    font-size: 0.36rem;
    line-height: 5.76rem;
    border-radius: 6px;
    color: #333;
    text-align: center;
}

.content{
    width: 4.6rem;
    overflow: hidden;
    float: left;
    margin-left: 1.8rem;
}
.upload{
    width: 100%;
    height: 2.4rem;
    border: 1px dashed #ddd;
    text-align: center;
    padding:0 .58rem;
    border-radius: 4px;
}
.icon{
    width: .6rem;
    height: .6rem;
    margin: .4rem auto;
}
.load-title{
    font-size: .18rem;
    color: #666;
    margin-bottom: .1rem;
}
.marks{
    font-size: .16rem;
    color: #999;
    width: 100%;
    overflow:hidden;
    white-space: normal;
    text-overflow: ellipsis;
}
.options{
    display: flex;
    justify-content: space-around;
    font-size: 0;
    margin-top: .3rem;
    margin-bottom: .5rem;
}
.selected{
    background: url(../images/selected_blue_icon@2x.png) 0 0 no-repeat!important;
}
.options p i{
    display: inline-block;
    width: .18rem;
    height: .18rem;
    margin-right: .12rem;
    background: url(../images/unselected_gray_icon@2x.png) 0 0 no-repeat;
    background-size: 100% 100%;
    vertical-align: sub;
}
.options p span{
    font-size: .18rem;
    color: #666;
    font-weight: 400;
}
.option-item .information input{
    width:100%;
    height: .5rem;
    padding:.16rem .4rem .16rem .15rem;
    border: 1px solid #ddd;
    outline: none;
    margin-bottom: .4rem;
    font-size: .18rem;
    color: #999;
    border-radius: 4px;
    -webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,0.075);
    box-shadow: inset 0 1px 1px rgba(0,0,0,0.075);
}
.oaMain_list h1{
    font-size: 0.3rem;
    height: 0.36rem;
    color: #0084ff;
    text-align: center;
}
.oaMain_list img {
    width: 100%;
    height: auto;
    margin: 0 auto;
}
.option-item .information>p {
    width: 100%;
    font-size: 0.16rem;
    line-height: 0.18rem;
    margin-bottom: 0.06rem;
    color: #0084ff;
}
.option-item>.information>input[type=button],.option-item>.information>button {
    display: block;
    width: 2.2rem;
    height: 0.5rem;
    padding: 0;
    float: left;
    font-size: 0.2rem;
    border: 0.01rem solid #d9d9d9;
    background: #fff;
    color: #0084ff;
    text-align: center;
    border-radius: 4px;
    cursor: pointer;
}
.option-item>.information>button {
    background-color: #0084ff;
    border-color: #0084ff;
    color: #fff;
    float: right;
}
#markss{
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;left: 0;
    cursor:pointer;
}
.upurl{
    display: none;
}
.upurl>button {
    width: 100%;
    display: block;
    height: 0.5rem;
    padding: 0;
    font-size: 0.2rem;
    border: 0.01rem solid #d9d9d9;
    background: #0084ff;
    color: #fff;
    text-align: center;
    border-radius: 4px;
    cursor: pointer;
    margin-bottom: 3rem;
}
/*下载缓冲窗*/
 .oaMain_loading {
     display: none;
