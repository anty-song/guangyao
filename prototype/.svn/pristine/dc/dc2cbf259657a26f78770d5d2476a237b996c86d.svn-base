.replacePwdBox {
    display: none;
    width: 100%;
    height: 100%;
    background: rgba(100,100,100,0.2);
    top: 0;
    left: 0;
    position: absolute;
    z-index: 1002;
}
.mylayer,
.mylayer1,
.mylayer2{
    width: 5.6rem;
    height: 3.4rem;
    position: absolute;
    top:50%;
    left: 50%;
    margin: -1.7rem 0 0 -2.8rem;
    padding:.3rem .5rem;
    line-height: .22rem;
    color: #333;
    font-weight: 400;
    background: #fff;
    z-index: 200;
    text-align: center;
    display: none;
    overflow: hidden;
}
.mylayer{text-align: left;}
.mylayer1{text-align: center;}
.mylayer>img,
.mylayer1>img,
.mylayer2>img{
    width: .2rem;
    height: .2rem;
