.bottombar {
  color: #bdbdbd;
  display: flex;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 98rpx;
  border-top: 1rpx solid #f0f0f0;
  background-color: #fff;
}
.bottombar view {
  display: flex;
  flex-direction: column;
  flex-grow: 1;
  justify-content: space-around;
  align-items: center;
}
.bottombar .icon {
  font-size: 42rpx;
}
.bottombar text {
  font-size: 23rpx;
}