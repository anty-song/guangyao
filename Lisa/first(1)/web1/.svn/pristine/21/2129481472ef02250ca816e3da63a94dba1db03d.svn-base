html{height:100%}
body{height:100%}
.main{min-height: 100%;}

.logo1{background-color: #fff;}
.logo{height:70px;position: relative;}
.logo .logo_wb{width:1200px;position: absolute;top:50%;left: 50%;transform: translate(-50%,-50%);}
.logo .logo_wb .logolf{float:left;font-size: 0;}
.logo .logo_wb .logort{float: right;}
.logo .logo_wb .logolf .logoimg{width:70px;height: 41px;display: inline-block;}
.logo .logo_wb .logolf .logoimg img{width:100%;height:100%}
.logo .logo_wb .logolf .logowz{display: inline-block;margin-left: 5px;}
.logo .logo_wb .logolf .logowz h3{font-size: 18px;letter-spacing: 1px;color: #2b2b2b;margin-bottom: 5px;}
.logo .logo_wb .logolf .logowz span{font-size: 12px;color: #8a8a8a;}

.logo .logo_wb .logort.wz{height:40px;line-height:40px;font-size: 16px;color: #555;}
.navbar{border-radius:0px;}
.navbar-collapse{padding-right:0;padding-left: 0;}
.navbar-default{background-color: #E62F2F;border: none;}
.navbar{margin-bottom: 0px;}
.navbar-default .navbar-nav>li{margin-right: 60px;}
.navbar-default .navbar-nav>li:last-child{margin-right: 0px;}
.navbar-default .navbar-nav>li>a{font-size:16px;color:#FFF;padding:15px 63px;}
.navbar-default .navbar-nav>li>a:hover{color:#FFF;}
.navbar-default .navbar-nav>.active>a{color:#FFF;background-color: #A31A1A}
.navbar-default .navbar-nav>.active>a:hover{color:#FFF;background-color: #A31A1A}

.swiper-container{width:100%;overflow: hidden;}
.swiper-container .swiper-slide{align-items: center;justify-content: center;display: flex;display: -webkit-flex;    overflow: hidden;}

.swiper-container1{width:100%;overflow: hidden;position: relative;}
.swiper-container1 .swiper-slide img{width:100%}

section{padding-bottom: 62px;}
section .tit{text-align: center;margin-top: 60px;margin-bottom: 50px;}
section .titone{font-size: 0;}
section .tit .titone h2{display: inline-block;font-size: 26px;color:#e62f2f;margin:auto 20px;}
section .tit .titone .tit_hr{width:110px;height: 1px;background-color: #ccc;border:none;display:inline-block;margin:0;}
section .tit p{font-size: 14px;color:#555;margin-top: 15px;}

section.banner{padding-bottom: 0;}

section .more {margin-top:55px;text-align: center;}
section .more a{width:140px;height:50px;line-height:50px;text-align:center;display:inline-block;font-size: 14px;color: #555;border: 1px solid #888;}
section .more a:hover{border:1px solid #E62F2F;background-color: #E62F2F;color: #fff;}

/*服务项目*/
section.fwxm ul.fwxmlist li{width:300px;height: 185px;}
section.fwxm ul.fwxmlist li.wb{position: relative;background-color: rgba(244,243,243,1);}
section.fwxm ul.fwxmlist li.wb .ms{position: absolute;top:50%;left:50%;transform: translate(-50%,-50%);}
section.fwxm ul.fwxmlist li.wb .ms h4{font-size: 16px;color: #333;text-align: center;margin-bottom: 18px;}
section.fwxm ul.fwxmlist li.wb .ms p{width:196px;font-size:14px;line-height: 30px;color: #555;display: -webkit-box;-webkit-line-clamp: 2;-webkit-box-orient: vertical;overflow: hidden;}
section.fwxm ul.fwxmlist li.pic{width:300px;height: 185px;overflow: hidden;}
section.fwxm ul.fwxmlist li.pic img{width:100%;height: 100%;transition:all 0.5s;}
section.fwxm ul.fwxmlist li.pic img:hover{transform: scale(1.05);}

section.fwxm ul.fwxmlist li.wb .ms  a:hover h4{color:#A31A1A}
section.fwxm ul.fwxmlist li.wb .ms  a:hover p{color:#A31A1A ;}

/*新闻动态*/
section.news{background-color: rgba(244,243,243,1);}
/* section.news ul.newslist{width:100%;height: 405px;background-color: #fff;padding:30px 40px;} */
section.news ul.newslist{width:100%;background-color: #fff;padding:30px 40px;}
section.news ul.newslist li{width:100%;margin-bottom: 27px;    font-size: 0;}
section.news ul.newslist li i{width:8px;height: 8px;margin-right:5px;display:inline-block;background-color: #e62f2f;border-radius: 50%;}
section.news ul.newslist li a h1{color:#333;display: inline-block;width: 80%;font-size:16px;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
section.news ul.newslist li a time{float: right;color:#888;font-size: 14px;}

section.news ul.newslist li  a:hover h1{color:#A31A1A}
section.news ul.newslist li  a:hover time{color:#A31A1A ;}

section.news ul.newslist li  a:active h1{color:#0461B1}
section.news ul.newslist li  a:active time{color:#0461B1 ;}

/*案例展示*/
section.alzs ul.alzslist{}
section.alzs ul.alzslist li{margin-bottom: 20px;}
section.alzs ul.alzslist li .bor{width:285px;height: 310px;border:1px solid #ccc;position: relative;}
section.alzs ul.alzslist li .bor dl{width:264px;position: absolute;top:50%;left: 50%;transform: translate(-50%,-50%);}
section.alzs ul.alzslist li .bor dl dt{width:100%;height: 190px;overflow: hidden;}
section.alzs ul.alzslist li .bor dl dt img{width:100%;height: 100%;transition:all 0.5s;}
section.alzs ul.alzslist li .bor dl dt img:hover{transform: scale(1.05);}
section.alzs ul.alzslist li .bor dl dd{text-align: center;font-size: 14px;}
section.alzs ul.alzslist li .bor dl dd h4{width:100%;font-size: 16px;margin:20px auto 15px auto;color:#353535;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
section.alzs ul.alzslist li .bor dl dd p{line-height: 24px;color:#555;display: -webkit-box;-webkit-line-clamp: 2;-webkit-box-orient: vertical;overflow: hidden;}

section.alzs ul.alzslist li .bor a:hover h4{color:#A31A1A}
section.alzs ul.alzslist li .bor a:hover p{color:#A31A1A ;}

/*联系我们*/
section.contactus .tit{margin-bottom: 0px;}
section.contactus ul.conulist{margin-top: -35px;}
section.contactus ul.conulist li {}
section.contactus ul.conulist li .all{width:285px;height:320px;position: relative;}
section.contactus ul.conulist li .all .bj{position: absolute; bottom:0;width:100%;height: 130px;background-color: #e62f2f;}
section.contactus ul.conulist li .all .con{position: absolute;bottom:8%;width: 100%; text-align: center;font-size: 0;}
section.contactus ul.conulist li .all .con span{margin:10px auto;display: inline-block;font-size: 14px;}
section.contactus ul.conulist li .all .con .erweima{width:120px;height: 120px; margin: 0 auto;padding:3px;background-color: #fff;}
section.contactus ul.conulist li .all .con .erweima img{width:100%;height: 100%;}
section.contactus ul.conulist li .all .con h4{color: #fff;margin:10px auto ;font-size: 16px;}
section.contactus ul.conulist li .all .con .tel i{width:12px;height:14px;margin-right:10px;display: inline-block;background-image: url(../images/index_dianhua_icon.png);}
section.contactus ul.conulist li .all .con .tel a{color: #fff;font-size: 14px;}

footer{height:40px;line-height:40px;margin-top:-40px;background-color: #333;}
footer p{color: #fff;font-size: 14px;text-align: center;}


.service-ctt a {
    display: block;
}
.service-ctt a:hover .service-item-title,
.service-ctt a:hover .service-item-explains {
    color: #A31A1A;
}
.service-item-word {
    position: relative;
    height: 185px;
    background-color: rgba(244,243,243,1);
}
.service-item-word-ctt {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}
.service-item-pic {
    overflow: hidden;
    height: 185px;
}
.service-item-pic img {
    width: 100%;
    height: 100%;
    transition: all .5s;
}
.service-item-pic img:hover {
    transform: scale(1.05)
}
.service-item-title {
    font-size: 16px;
    color: #333;
}
.service-item-explains {
    font-size: 14px;
    line-height: 30px;
    color: #555;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
}
/* song 添加蒙层 */
.songmask {
    position: fixed;
    z-index: 1000;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    text-align: center;
    background: #fff;
}
.songmask img {
    display: block;
    position: absolute;
    top: 50%;
    left: 50%;
    width: 50px;
    height: 50px;
    margin-left: -25px;
    margin-top: -25px;
}
#preloader_1{
    position:relative;
    top:50%;
    left: 50%;
    width:45px;
    transform: translate(-50%,-50%);
}
#preloader_1 span{
    display:block;
    bottom:0px;
    width: 9px;
    height: 5px;
    background:#9b59b6;
    position:absolute;
    animation: preloader_1 1.5s  infinite ease-in-out;
}
 
#preloader_1 span:nth-child(2){
left:11px;
animation-delay: .2s;
 
}
#preloader_1 span:nth-child(3){
left:22px;
animation-delay: .4s;
}
#preloader_1 span:nth-child(4){
left:33px;
animation-delay: .6s;
}
#preloader_1 span:nth-child(5){
left:44px;
animation-delay: .8s;
}
@keyframes preloader_1 {
    0% {height:5px;transform:translateY(0px);background:#9b59b6;}
    25% {height:30px;transform:translateY(15px);background:#3498db;}
    50% {height:5px;transform:translateY(0px);background:#9b59b6;}
    100% {height:5px;transform:translateY(0px);background:#9b59b6;}
}
