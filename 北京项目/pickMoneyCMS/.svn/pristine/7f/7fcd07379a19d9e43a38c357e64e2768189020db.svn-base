            "slide_image.png",
            "startBtn.png",
            "timg.jpg",
            "yello_light.png"
        ]
    });
    var image_num=0;
    var tick;
    tick=setInterval(function(){
        var img=new Image();
        img.src=path.src+image_arr.image[image_num];
