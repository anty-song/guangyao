		<style type="text/css">
			.pagination {
			  position: absolute;
			  z-index: 20;
			  bottom: 10px;
			  width: 100%;
			  text-align: center;
			}
			.pagination2 {
			  position: absolute;
			  z-index: 20;
			  bottom: 10px;
			  width: 100%;
			  text-align: center;
			}
			.swiper-pagination-switch {
			  display: inline-block;
			  width: 8px;
			  height: 8px;
			  border-radius: 8px;
			  background: #555;
			  margin: 0 5px;
			  opacity: 0.8;
			  border: 1px solid #fff;
			  cursor: pointer;
			}
			.swiper-active-switch {
			  background: #fff;
			}
			.swiper-button-prev{
				position: absolute;
			    width: 60px;
			    height: 60px;
			    top: 50%;
			    left: 0;
			    transform: translate(0%,-50%);
		    }
			.swiper-button-next{
				position: absolute;
			    width: 60px;
			    height: 60px;
			    top: 50%;
			    right:0;
			    transform: translate(0%,-50%);
				}
			.swiper-button-prev img{width:100%;height: 100%;}
			.swiper-button-next img{width:100%;height: 100%;}
		</style>	
		
		<script type="text/javascript">
			window.onload = function() {
			  var mySwiper = new Swiper('.swiper-container',{
				    loop: true,
				    mode:'horizontal',
				    resizeReInit : true,
				    calculateHeight : true,
				    autoplay: 0,
				    speed: 300,
				   	pagination : '.pagination',
					createPagination :true,
					
					onInit: function(swiper){
				      swiper.swipeNext()
				    }
				    //其他设置
				});  
				
//				$('.swiper-button-prev').click(function(){
//					mySwiper.swipePrev(); 
//				})
//				
//				$('.swiper-button-next').click(function(){
//					mySwiper.swipeNext(); 
//				})
			}
		</script>
		<script>
			var config = new Config();
			new Vue({
				el: "#alzs",
				data: {
					logo:{},
					caseindata: {}
				},
				created: function () {
					var that = this;
					$.ajax({
						type:config.caselist.type,
						url: config.caselist.url,
						data:{
							action:"case_list"
						},
						async:true,
						dataType: "json",
						success: function (res) {
							var slideshow = res.data.logo.slideshow;
							console.log(typeof slideshow);
							console.log(res);
							that.caseindata=res.data.casedata;
							console.log(res.data);
							that.logo = res.data.logo;
						},
						complete: function(data) {
							setTimeout(function() {
								$(".songmask").remove();
							}, 500);
						},
						error: function() {
							
						}
					})
				}
			})
		</script>
