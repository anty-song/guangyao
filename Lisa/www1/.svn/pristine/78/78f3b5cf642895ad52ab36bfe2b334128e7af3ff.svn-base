@media (min-width: 1200px){
	.container {width: 1200px;}
}

@media screen and (max-width:1024px){
	.banners{overflow: hidden;}
	header.fir{height: auto;}
	header.fir nav{position: inherit;}
	header.fir .banner{height: auto;position: inherit;}
	.navbar{margin-bottom: 0;}
	section.ywfw .con{height: auto;position: static;}
	section.ywfw .con .ullist{position: static;transform: inherit;}
	section.ywfw .tit{height:120px;}
	section.ywfw .tit h1{padding:15px 0 7px 0;font-size: 0.3rem;}
	section.ywfw .con ul.ywlist{padding:30px 0;}
	section.ywfw .con ul.ywlist li{margin: 0;}
	section.ywfw .con ul.ywlist li dl dt{width:100%;height:100%;}
	section.ywfw .con ul.ywlist li dl dd{width: 100%;}
	section.ywfw .con ul.ywlist li dl dd p{width: 100%;}
	
	
	section.connews{text-align: center;}
	section.connews .con .newspic dl{width:80%;margin: 0 auto;}
	section.connews .con .newspic dl dt{width: 100%;height: 100%;}
	section.connews .con .newspic dl dd{width:100%;}
	section.connews .con .newspic dd p{width:100%;text-align: left;}
	
	section.connews .con .newslist{width: 80%;margin-top: 30px;}
	section.connews .con .newslist li{text-align: left;float:none}
	section.connews .con .newslist li time{width:6%;}
	section.connews .con .newslist li .newlistcon{width:80%;}
	section.connews .con .newslist li .newlistcon h3{width:100%}
	section.connews .con .newslist li .newlistcon p{width:100%}
	
	section.contactus .conuslist li .licon{width:100%}
	
	section.newsxq .wb{padding-top: 0.3rem;}
	section.newsxq .wb h3{margin-bottom: 0.4rem;}
	section.newsxq .wb p{text-indent: 0;}
	section.newsxq .wb .read{margin-bottom: 0.5rem;}
	
	
	section.connews .con .newslist li .newlistcon{width:70%;}
	section.connews .con .newslist li .newlistcon h3{width:auto;}
	
	section.content ul.newslist li{width:100%;height:auto}
	section.content ul.newslist li dl{width:95%}
	section.content ul.newslist li dl dd{width:100%;margin-left: 0;}
	section.content ul.newslist li.havepic dl dt{margin-right: 0.1rem;}
	section.content ul.newslist li.havepic dl dd{width:75%}
	section.content ul.newslist li.nopic   dl dd{width:100%}
	
	section.content ul.newslist li.havepic dl dd p{width:100%}
	section.content ul.newslist li.nopic   dl dd p{width:100%}
	
	section.content .fy ul.fyq{padding:20px 0 60px 0}
}


@media screen and (max-width:992px){
	
	section.connews .con .newslist{width: 100%;}
	section.connews .con .newslist li{width: 100%;}
	
	}
	
@media screen and (max-width:768px){
	.container {width: 750px;}
	.banners{display: block;overflow: hidden;}
	.banner{width:100%;overflow: hidden;/*align-items: center;justify-content: center; display: flex;display: -webkit-flex;*/}
	section.ywfw .tit{height: 70px;}
	section.ywfw .tit h1{padding: 0;font-size:0.18rem}
	section.ywfw .con ul.ywlist li dd h2{font-size: 0.17rem;}
	section.ywfw .con ul.ywlist li dl {font-size: 0.14rem;}
	
	
	section .tit hr{margin:0 auto}
	
	section.connews .tit h1{font-size:0.18rem;margin-top:0px;}
	section.connews .con .newspic dl dd h3{font-size: 0.16rem;}
	section.connews .con .newspic dl dd p{font-size:0.14rem}
	section.connews .con .newslist li time span.day{font-size: 0.36rem;}
	section.connews .con .newslist li time span.rq{font-size: 0.12rem;}
	section.connews .con .newslist li .newlistcon h3{font-size: 0.16rem;}
	section.connews .con .newslist li .newlistcon p{font-size: 0.14rem;}
	
	section.connews .more{width:100px;height:40px;line-height:40px;margin:0px auto 40px auto}
	section.connews .more a{font-size: 0.14rem;}
	
	section.contactus .tit{padding:20px 0 40px 0;}
	section.contactus .tit h1{font-size:0.18rem}
	section.contactus .conuslist li .lxrxx h4{font-size: 0.16rem;}
	section.contactus .conuslist li .lxrxx span.tel{font-size: 0.13rem;}
	section.contactus .conuslist li .licon p{font-size: 0.13rem;}
	
	section.content ul.newslist li.havepic dl dd{width:67%}
	
	section.newsxq .wb h3{font-size: 0.15rem;}
	section.newsxq .wb .read span{font-size:0.12rem}
	section.newsxq .wb p{font-size: 0.10rem;}
	
	div.text-center{padding: 20px 0 10px 0;}
	
}


@media screen and (max-width:480px){
	header.fir nav.navbar-default{background-color: rgba(255,255,255,1)!important;}
	header.fir nav{position: fixed}
	.swiper-container1{padding-top: 70px;}
	/*header.fir .banner{padding-top: 70px;}*/
	.container {width: 450px;}
	section.ywfw .tit{height:auto;background-image:none ;background-color: #F3F4F5;;}

	section.ywfw .tit h1{padding: 15px 0 7px 0;font-size:0.35rem;padding: 0;color:#353535;}
	section.ywfw .tit hr{background-color:#0363B2!important;}
	section.connews .con .newspic {display: inline-block;width:100%}
	section.connews .con .newspic dl dd h3{font-size: 0.3rem;}
	section.connews .con .newspic dl dd p{font-size:0.26rem}
	section.connews .con .newslist li time{width:20%;text-align: center;margin: 0;}
	section.connews .con .newslist li time span.day{font-size: 0.6rem;}
	section.connews .con .newslist li time span.rq{font-size: 0.24rem;}
	section.connews .con .newslist li .newlistcon h3{font-size: 0.3rem;}
	section.connews .con .newslist li .newlistcon p{font-size: 0.26rem;}
	
	section.connews .tit{margin:30px auto 20px auto}
	section.connews .tit h1{font-size:0.35rem;padding-bottom: 0;}
	section.connews .more{width:110px;height: 34px;line-height:34px;margin:0px auto 30px auto }
	section.connews .more a{font-size: 0.28rem;}
	
	section.contactus .tit{padding:20px 0 30px 0}
	section.contactus .tit h1{font-size:0.35rem;padding:0;}
	
	section.contactus .conuslist li .lxrxx h4{font-size: 0.32rem;}
	section.contactus .conuslist li .lxrxx span.tel{font-size: 0.26rem;}
	section.contactus .conuslist li .licon p{font-size: 0.26rem;}
	
	section.ywfw .con ul.ywlist li dl dd h2{font-size: 0.33rem;}
	
	section.newsxq .wb h3{font-size: 0.33rem;}
	section.newsxq .wb p{font-size: 0.26rem;    letter-spacing: 1px;}
	section.newsxq .wb .read span{font-size: 0.24rem;}
	section.content ul.newslist li dl dd h4{font-size: 0.32rem;padding:0;letter-spacing: 0.01rem;line-height: 0.5rem;}
	section.content ul.newslist li dl dd p{font-size: 0.28rem;}
	

}

@media screen and (max-width:425px){
	.container {width: 400px;}
	
	header.fir nav ul.navlist{width:100%;background-color: #fff;margin:0}
	header.fir nav ul.navlist li{width:100%;margin: 0;border-bottom:1px solid #eee;}
	header.fir nav ul.navlist li a{font-size: 0.3rem;color: #333;padding:0.2rem 0.5rem;text-align: right;}
	header.fir nav ul.navlist li a.lihov{background-color: #FFF;color: #0363b2;border-radius: 10px;}
	.navbar-default{border-color:#fff}
	.navbar-header{width:96%;margin:0 auto!important;}
	.navbar-toggle{margin-top: 15px;}
	.container > .navbar-collapse{padding:0;margin-right:0px;margin-left:0px;}
	
	/*section.ywfw .tit h1{padding:30px 0 7px 0;}*/
	
	section.connews .con .newslist li .newlistcon{width:76%;}
	section.connews .con .newslist li .newlistcon h3{width:auto;}
	
	section.content ul.newslist li{width:100%;height:auto}
	section.content ul.newslist li dl{width:95%}
	section.content ul.newslist li dl dd{width:100%;margin-left: 0;}

	section.content ul.newslist li.havepic dl dt{display: inherit;margin: 20px auto;}
	section.content ul.newslist li.havepic dl dd{width:100%}
	section.content ul.newslist li.nopic   dl dd{width:100%}
	
	section.content ul.newslist li.havepic dl dd p{width:100%}
	section.content ul.newslist li.nopic   dl dd p{width:100%}
	
	section.content .fy ul.fyq{padding:20px 0 60px 0}
	
	
}

@media screen and (max-width:375px){.container {width: 350px;}}

